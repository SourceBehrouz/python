    <sentence id="DDI-DrugBank.d431.s8" text="Therefore, proton pump inhibitors should be taken at least 30 minutes prior to sucralfate.">
        <entity charOffset="11-32" id="DDI-DrugBank.d431.s8.e0" text="proton pump inhibitors" type="group"/>
