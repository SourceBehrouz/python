        <pair ddi="false" e1="DDI-DrugBank.d522.s12.e0" e2="DDI-DrugBank.d522.s12.e2" id="DDI-DrugBank.d522.s12.p1"/>
        <pair ddi="false" e1="DDI-DrugBank.d522.s12.e0" e2="DDI-DrugBank.d522.s12.e3" id="DDI-DrugBank.d522.s12.p2"/>
        <pair ddi="false" e1="DDI-DrugBank.d522.s12.e1" e2="DDI-DrugBank.d522.s12.e2" id="DDI-DrugBank.d522.s12.p3"/>
