        <entity charOffset="56-68" id="DDI-DrugBank.d522.s12.e1" text="dirithromycin" type="drug"/>
        <entity charOffset="114-125" id="DDI-DrugBank.d522.s12.e2" text="theophylline" type="drug"/>
        <entity charOffset="237-248" id="DDI-DrugBank.d522.s12.e3" text="theophylline" type="drug"/>
