        <pair ddi="false" e1="DDI-DrugBank.d522.s3.e2" e2="DDI-DrugBank.d522.s3.e3" id="DDI-DrugBank.d522.s3.p5"/>
    </sentence>
    <sentence id="DDI-DrugBank.d522.s4" text="In five men, terfenadine levels were undetectable ( 5 ng/mL) throughout the study;">
        <entity charOffset="13-23" id="DDI-DrugBank.d522.s4.e0" text="terfenadine" type="drug"/>
    </sentence>
    <sentence id="DDI-DrugBank.d522.s5" text="in one man, the C max of terfenadine was 8.1 ng/mL with terfenadine alone and 7.2 ng/mL with terfenadine plus dirithromycin.">
