        <entity charOffset="143-153" id="DDI-DrugBank.d522.s3.e1" text="terfenadine" type="drug"/>
        <entity charOffset="171-181" id="DDI-DrugBank.d522.s3.e2" text="terfenadine" type="drug"/>
        <entity charOffset="188-200" id="DDI-DrugBank.d522.s3.e3" text="dirithromycin" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d522.s3.e0" e2="DDI-DrugBank.d522.s3.e1" id="DDI-DrugBank.d522.s3.p0"/>
