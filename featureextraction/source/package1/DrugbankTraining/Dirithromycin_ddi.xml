    <sentence id="DDI-DrugBank.d522.s0" text="Terfenadine: In a prospective study involving six-healthy-male volunteers, dirithromycin did not affect the metabolism of terfenadine.">
        <entity charOffset="0-10" id="DDI-DrugBank.d522.s0.e0" text="Terfenadine" type="drug"/>
        <entity charOffset="75-87" id="DDI-DrugBank.d522.s0.e1" text="dirithromycin" type="drug"/>
