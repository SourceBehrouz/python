        <pair ddi="true" e1="DDI-DrugBank.d546.s0.e1" e2="DDI-DrugBank.d546.s0.e2" id="DDI-DrugBank.d546.s0.p2" type="advise"/>
    </sentence>
    <sentence id="DDI-DrugBank.d546.s1" text="If the drugs are used together, caution should be exercised because unexpected hypotension could result from beta-blocker inhibition of the sympathetic reflex response to fenoldopam.">
        <entity charOffset="171-180" id="DDI-DrugBank.d546.s1.e0" text="fenoldopam" type="drug"/>
    </sentence>
    <sentence id="DDI-DrugBank.d546.s2" text="Drug Interactions, General: Although there have been no formal interaction studies, intravenous fenoldopam has been administered safely with drugs such as digitalis and sublingual nitroglycerin.">
        <entity charOffset="96-105" id="DDI-DrugBank.d546.s2.e0" text="fenoldopam" type="drug"/>
        <entity charOffset="155-163" id="DDI-DrugBank.d546.s2.e1" text="digitalis" type="group"/>
        <entity charOffset="180-192" id="DDI-DrugBank.d546.s2.e2" text="nitroglycerin" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d546.s2.e0" e2="DDI-DrugBank.d546.s2.e1" id="DDI-DrugBank.d546.s2.p0"/>
