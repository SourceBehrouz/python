    <sentence id="DDI-DrugBank.d546.s0" text="Drug Interactions with Beta-Blockers: Concomitant use of fenoldopam with beta-blockers should be avoided.">
        <entity charOffset="23-35" id="DDI-DrugBank.d546.s0.e0" text="Beta-Blockers" type="group"/>
        <entity charOffset="57-66" id="DDI-DrugBank.d546.s0.e1" text="fenoldopam" type="drug"/>
        <entity charOffset="73-85" id="DDI-DrugBank.d546.s0.e2" text="beta-blockers" type="group"/>
        <pair ddi="false" e1="DDI-DrugBank.d546.s0.e0" e2="DDI-DrugBank.d546.s0.e1" id="DDI-DrugBank.d546.s0.p0"/>
