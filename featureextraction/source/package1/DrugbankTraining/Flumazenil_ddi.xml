    <sentence id="DDI-DrugBank.d234.s0" text="Interaction with central nervous system depressants other than benzodiazepines has not been specifically studied;">
        <entity charOffset="17-50" id="DDI-DrugBank.d234.s0.e0" text="central nervous system depressants" type="group"/>
        <entity charOffset="63-77" id="DDI-DrugBank.d234.s0.e1" text="benzodiazepines" type="group"/>
        <pair ddi="false" e1="DDI-DrugBank.d234.s0.e0" e2="DDI-DrugBank.d234.s0.e1" id="DDI-DrugBank.d234.s0.p0"/>
    <negationtags>DDI-DrugBank.d234.s0</negationtags></sentence>
    <sentence id="DDI-DrugBank.d234.s1" text="however, no deleterious interactions were seen when ROMAZICON was administered after narcotics, inhalational anesthetics, muscle relaxants and muscle relaxant antagonists administered in conjunction with sedation or anesthesia.">
        <entity charOffset="52-60" id="DDI-DrugBank.d234.s1.e0" text="ROMAZICON" type="brand"/>
        <entity charOffset="85-93" id="DDI-DrugBank.d234.s1.e1" text="narcotics" type="group"/>
        <entity charOffset="109-119" id="DDI-DrugBank.d234.s1.e2" text="anesthetics" type="group"/>
        <entity charOffset="122-137" id="DDI-DrugBank.d234.s1.e3" text="muscle relaxants" type="group"/>
        <pair ddi="false" e1="DDI-DrugBank.d234.s1.e0" e2="DDI-DrugBank.d234.s1.e1" id="DDI-DrugBank.d234.s1.p0"/>
        <pair ddi="false" e1="DDI-DrugBank.d234.s1.e0" e2="DDI-DrugBank.d234.s1.e2" id="DDI-DrugBank.d234.s1.p1"/>
        <pair ddi="false" e1="DDI-DrugBank.d234.s1.e0" e2="DDI-DrugBank.d234.s1.e3" id="DDI-DrugBank.d234.s1.p2"/>
        <pair ddi="false" e1="DDI-DrugBank.d234.s1.e1" e2="DDI-DrugBank.d234.s1.e2" id="DDI-DrugBank.d234.s1.p3"/>
        <pair ddi="false" e1="DDI-DrugBank.d234.s1.e1" e2="DDI-DrugBank.d234.s1.e3" id="DDI-DrugBank.d234.s1.p4"/>
