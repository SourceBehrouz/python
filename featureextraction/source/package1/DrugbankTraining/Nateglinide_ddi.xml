    <sentence id="DDI-DrugBank.d460.s0" text="In vitro drug metabolism studies indicate that Starlix is predominantly metabolized by the cytochrome P450 isozyme CYP2C9 (70%) and to a lesser extent CYP3A4 (30%).">
        <entity charOffset="47-53" id="DDI-DrugBank.d460.s0.e0" text="Starlix" type="brand"/>
    </sentence>
    <sentence id="DDI-DrugBank.d460.s1" text="Starlix is a potential inhibitor of the CYP2C9 isoenzyme in vivo as indicated by its ability to inhibit the in vitro metabolism of tolbutamide.">
        <entity charOffset="0-6" id="DDI-DrugBank.d460.s1.e0" text="Starlix" type="brand"/>
        <entity charOffset="131-141" id="DDI-DrugBank.d460.s1.e1" text="tolbutamide" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d460.s1.e0" e2="DDI-DrugBank.d460.s1.e1" id="DDI-DrugBank.d460.s1.p0" type="mechanism"/>
    </sentence>
    <sentence id="DDI-DrugBank.d460.s2" text="Inhibition of CYP3A4 metabolic reactions was not detected in in vitro experiments."><negationtags>Inhibition of CYP3A4 metabolic reactions was &lt;scope&gt; &lt;cue&gt; not &lt;/cue&gt; detected in in vitro experiments. &lt;/scope&gt;</negationtags></sentence>
    <sentence id="DDI-DrugBank.d460.s3" text="Glyburide: In a randomized, multiple-dose crossover study, patients with Type 2 diabetes were administered 120 mg Starlix three times a day before meals for 1 day in combination with glyburide 10 mg daily.">
        <entity charOffset="0-8" id="DDI-DrugBank.d460.s3.e0" text="Glyburide" type="drug"/>
        <entity charOffset="114-120" id="DDI-DrugBank.d460.s3.e1" text="Starlix" type="brand"/>
