        <pair ddi="false" e1="DDI-DrugBank.d440.s4.e1" e2="DDI-DrugBank.d440.s4.e2" id="DDI-DrugBank.d440.s4.p2"/>
    </sentence>
    <sentence id="DDI-DrugBank.d440.s5" text="Among these patients, the incidence of upper gastrointestinal adverse experiences in the patients treated with Ibandronate was similar to that in placebo-treated patients.">
        <entity charOffset="111-121" id="DDI-DrugBank.d440.s5.e0" text="Ibandronate" type="drug"/>
    </sentence>
    <sentence id="DDI-DrugBank.d440.s6" text="Similarly, of over 1600 patients enrolled in a study comparing once-monthly with daily dosing regimens of ibandronate, 14% of patients used anti-peptic agents.">
        <entity charOffset="106-116" id="DDI-DrugBank.d440.s6.e0" text="ibandronate" type="drug"/>
    </sentence>
    <sentence id="DDI-DrugBank.d440.s7" text="Among these patients, the incidence of upper gastrointestinal adverse experiences in the patients treated with Ibandronate 150 mg once monthly was similar to that in patients treated with Ibandronate 2.5 mg once daily.">
        <entity charOffset="111-121" id="DDI-DrugBank.d440.s7.e0" text="Ibandronate" type="drug"/>
        <entity charOffset="188-198" id="DDI-DrugBank.d440.s7.e1" text="Ibandronate" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d440.s7.e0" e2="DDI-DrugBank.d440.s7.e1" id="DDI-DrugBank.d440.s7.p0"/>
    </sentence>
    <sentence id="DDI-DrugBank.d440.s8" text="Aspirin/Nonsteroidal Antiinflammatory Drugs (NSAIDs)">
        <entity charOffset="0-6" id="DDI-DrugBank.d440.s8.e0" text="Aspirin" type="brand"/>
        <entity charOffset="8-41" id="DDI-DrugBank.d440.s8.e1" text="Nonsteroidal Antiinflammatory Drug" type="group"/>
        <entity charOffset="45-50" id="DDI-DrugBank.d440.s8.e2" text="NSAIDs" type="group"/>
        <pair ddi="false" e1="DDI-DrugBank.d440.s8.e0" e2="DDI-DrugBank.d440.s8.e1" id="DDI-DrugBank.d440.s8.p0"/>
