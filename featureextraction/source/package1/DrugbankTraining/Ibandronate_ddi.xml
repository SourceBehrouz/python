    <sentence id="DDI-DrugBank.d440.s4" text="Of over 3500 patients enrolled in the Ibandronate osteoporosis Treatment and Prevention Studies, 15% used anti-peptic agents (primarily H2 blockers and PPIs).">
        <entity charOffset="38-48" id="DDI-DrugBank.d440.s4.e0" text="Ibandronate" type="drug"/>
        <entity charOffset="136-146" id="DDI-DrugBank.d440.s4.e1" text="H2 blockers" type="group"/>
        <entity charOffset="152-155" id="DDI-DrugBank.d440.s4.e2" text="PPIs" type="group"/>
        <pair ddi="false" e1="DDI-DrugBank.d440.s4.e0" e2="DDI-DrugBank.d440.s4.e1" id="DDI-DrugBank.d440.s4.p0"/>
