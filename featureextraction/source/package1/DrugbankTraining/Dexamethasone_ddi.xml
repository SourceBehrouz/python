    </sentence>
    <sentence id="DDI-DrugBank.d314.s19" text="Drugs which inhibit CYP 3A4 (e.g., ketoconazole, macrolide antibiotics such as erythromycin) have the potential to result in increased plasma concentrations of corticosteroids.">
        <entity charOffset="35-46" id="DDI-DrugBank.d314.s19.e0" text="ketoconazole" type="drug"/>
        <entity charOffset="49-69" id="DDI-DrugBank.d314.s19.e1" text="macrolide antibiotics" type="group"/>
        <entity charOffset="79-90" id="DDI-DrugBank.d314.s19.e2" text="erythromycin" type="drug"/>
        <entity charOffset="160-174" id="DDI-DrugBank.d314.s19.e3" text="corticosteroids" type="group"/>
        <pair ddi="false" e1="DDI-DrugBank.d314.s19.e0" e2="DDI-DrugBank.d314.s19.e1" id="DDI-DrugBank.d314.s19.p0"/>
        <pair ddi="false" e1="DDI-DrugBank.d314.s19.e0" e2="DDI-DrugBank.d314.s19.e2" id="DDI-DrugBank.d314.s19.p1"/>
