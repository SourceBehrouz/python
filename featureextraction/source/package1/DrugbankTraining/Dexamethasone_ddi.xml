        <pair ddi="false" e1="DDI-DrugBank.d314.s18.e0" e2="DDI-DrugBank.d314.s18.e5" id="DDI-DrugBank.d314.s18.p4"/>
        <pair ddi="false" e1="DDI-DrugBank.d314.s18.e1" e2="DDI-DrugBank.d314.s18.e2" id="DDI-DrugBank.d314.s18.p5"/>
        <pair ddi="false" e1="DDI-DrugBank.d314.s18.e1" e2="DDI-DrugBank.d314.s18.e3" id="DDI-DrugBank.d314.s18.p6"/>
        <pair ddi="true" e1="DDI-DrugBank.d314.s18.e1" e2="DDI-DrugBank.d314.s18.e4" id="DDI-DrugBank.d314.s18.p7" type="mechanism"/>
