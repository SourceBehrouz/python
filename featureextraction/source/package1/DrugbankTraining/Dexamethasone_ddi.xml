    <sentence id="DDI-DrugBank.d314.s0" text="Aminoglutethimide: Aminoglutethimide may diminish adrenal suppression by corticosteroids.">
        <entity charOffset="0-16" id="DDI-DrugBank.d314.s0.e0" text="Aminoglutethimide" type="drug"/>
        <entity charOffset="19-35" id="DDI-DrugBank.d314.s0.e1" text="Aminoglutethimide" type="drug"/>
        <entity charOffset="73-87" id="DDI-DrugBank.d314.s0.e2" text="corticosteroids" type="group"/>
        <pair ddi="false" e1="DDI-DrugBank.d314.s0.e0" e2="DDI-DrugBank.d314.s0.e1" id="DDI-DrugBank.d314.s0.p0"/>
        <pair ddi="false" e1="DDI-DrugBank.d314.s0.e0" e2="DDI-DrugBank.d314.s0.e2" id="DDI-DrugBank.d314.s0.p1"/>
