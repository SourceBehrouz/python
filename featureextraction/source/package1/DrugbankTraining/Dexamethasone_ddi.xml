        <pair ddi="false" e1="DDI-DrugBank.d314.s18.e0" e2="DDI-DrugBank.d314.s18.e1" id="DDI-DrugBank.d314.s18.p0"/>
        <pair ddi="false" e1="DDI-DrugBank.d314.s18.e0" e2="DDI-DrugBank.d314.s18.e2" id="DDI-DrugBank.d314.s18.p1"/>
