    <sentence id="DDI-DrugBank.d219.s25" text="Thus, concomitant therapy with warfarin and Lodine should not require dosage adjustment of either drug.">
        <entity charOffset="31-38" id="DDI-DrugBank.d219.s25.e0" text="warfarin" type="drug"/>
        <entity charOffset="44-49" id="DDI-DrugBank.d219.s25.e1" text="Lodine" type="brand"/>
