    </sentence>
    <sentence id="DDI-DrugBank.d219.s1" text="This interaction should be given consideration in patients taking NSAIDs concomitantly with ACE-inhibitors.">
        <entity charOffset="66-71" id="DDI-DrugBank.d219.s1.e0" text="NSAIDs" type="group"/>
        <entity charOffset="92-105" id="DDI-DrugBank.d219.s1.e1" text="ACE-inhibitors" type="group"/>
        <pair ddi="true" e1="DDI-DrugBank.d219.s1.e0" e2="DDI-DrugBank.d219.s1.e1" id="DDI-DrugBank.d219.s1.p0" type="advise"/>
    </sentence>
    <sentence id="DDI-DrugBank.d219.s2" text="Antacids: The concomitant administration of antacids has no apparent effect on the extent of absorption of Lodine.">
        <entity charOffset="0-7" id="DDI-DrugBank.d219.s2.e0" text="Antacids" type="group"/>
        <entity charOffset="44-51" id="DDI-DrugBank.d219.s2.e1" text="antacids" type="group"/>
