    <sentence id="DDI-DrugBank.d485.s0" text="Ethinyl estradiol: Substrate of CYP3A4 (major), 3A5-7 (minor);">
        <entity charOffset="0-16" id="DDI-DrugBank.d485.s0.e0" text="Ethinyl estradiol" type="drug"/>
    </sentence>
    <sentence id="DDI-DrugBank.d485.s1" text="Inhibits CYP1A2 (weak), 2B6 (weak), 2C19 (weak), 3A4 (weak)."/>
    <sentence id="DDI-DrugBank.d485.s2" text="Acetaminophen: May increase plasma concentration of synthetic estrogens, possibly by inhibiting conjugation.">
        <entity charOffset="0-12" id="DDI-DrugBank.d485.s2.e0" text="Acetaminophen" type="drug"/>
        <entity charOffset="52-70" id="DDI-DrugBank.d485.s2.e1" text="synthetic estrogens" type="group"/>
