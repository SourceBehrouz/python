    </sentence>
    <sentence id="DDI-DrugBank.d485.s3" text="Combination hormonal contraceptives may also decrease the plasma concentration of acetaminophen.">
        <entity charOffset="12-34" id="DDI-DrugBank.d485.s3.e0" text="hormonal contraceptives" type="group"/>
        <entity charOffset="82-94" id="DDI-DrugBank.d485.s3.e1" text="acetaminophen" type="drug"/>
