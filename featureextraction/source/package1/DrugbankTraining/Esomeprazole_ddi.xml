    </sentence>
    <sentence id="DDI-DrugBank.d29.s7" text="Esomeprazole may potentially interfere with CYP2C19, the major esomeprazole metabolizing enzyme.">
        <entity charOffset="0-11" id="DDI-DrugBank.d29.s7.e0" text="Esomeprazole" type="drug"/>
        <entity charOffset="63-74" id="DDI-DrugBank.d29.s7.e1" text="esomeprazole" type="drug"/>
