        <pair ddi="false" e1="DDI-DrugBank.d499.s13.e1" e2="DDI-DrugBank.d499.s13.e3" id="DDI-DrugBank.d499.s13.p5"/>
        <pair ddi="false" e1="DDI-DrugBank.d499.s13.e1" e2="DDI-DrugBank.d499.s13.e4" id="DDI-DrugBank.d499.s13.p6"/>
