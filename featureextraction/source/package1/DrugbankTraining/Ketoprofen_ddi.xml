    <sentence id="DDI-DrugBank.d499.s4" text="2."/>
    <sentence id="DDI-DrugBank.d499.s5" text="Aspirin: Ketoprofen does not alter aspirin absorption;">
        <entity charOffset="0-6" id="DDI-DrugBank.d499.s5.e0" text="Aspirin" type="brand"/>
        <entity charOffset="9-18" id="DDI-DrugBank.d499.s5.e1" text="Ketoprofen" type="drug"/>
        <entity charOffset="35-41" id="DDI-DrugBank.d499.s5.e2" text="aspirin" type="brand"/>
        <pair ddi="false" e1="DDI-DrugBank.d499.s5.e0" e2="DDI-DrugBank.d499.s5.e1" id="DDI-DrugBank.d499.s5.p0"/>
