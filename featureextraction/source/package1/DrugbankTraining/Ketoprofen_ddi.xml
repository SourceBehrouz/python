        <pair ddi="false" e1="DDI-DrugBank.d499.s22.e0" e2="DDI-DrugBank.d499.s22.e2" id="DDI-DrugBank.d499.s22.p1"/>
        <pair ddi="false" e1="DDI-DrugBank.d499.s22.e0" e2="DDI-DrugBank.d499.s22.e3" id="DDI-DrugBank.d499.s22.p2"/>
        <pair ddi="false" e1="DDI-DrugBank.d499.s22.e1" e2="DDI-DrugBank.d499.s22.e2" id="DDI-DrugBank.d499.s22.p3"/>
        <pair ddi="true" e1="DDI-DrugBank.d499.s22.e1" e2="DDI-DrugBank.d499.s22.e3" id="DDI-DrugBank.d499.s22.p4" type="mechanism"/>
        <pair ddi="true" e1="DDI-DrugBank.d499.s22.e2" e2="DDI-DrugBank.d499.s22.e3" id="DDI-DrugBank.d499.s22.p5" type="mechanism"/>
    </sentence>
    <sentence id="DDI-DrugBank.d499.s23" text="8."/>
    <sentence id="DDI-DrugBank.d499.s24" text="Lithium: Nonsteroidal anti-inflammatory agents have been reported to increase steadystate plasma lithium levels.">
        <entity charOffset="0-6" id="DDI-DrugBank.d499.s24.e0" text="Lithium" type="drug"/>
        <entity charOffset="9-45" id="DDI-DrugBank.d499.s24.e1" text="Nonsteroidal anti-inflammatory agents" type="group"/>
        <entity charOffset="97-103" id="DDI-DrugBank.d499.s24.e2" text="lithium" type="drug"/>
