        <pair ddi="false" e1="DDI-DrugBank.d499.s6.e1" e2="DDI-DrugBank.d499.s6.e3" id="DDI-DrugBank.d499.s6.p5"/>
        <pair ddi="false" e1="DDI-DrugBank.d499.s6.e1" e2="DDI-DrugBank.d499.s6.e4" id="DDI-DrugBank.d499.s6.p6"/>
        <pair ddi="false" e1="DDI-DrugBank.d499.s6.e2" e2="DDI-DrugBank.d499.s6.e3" id="DDI-DrugBank.d499.s6.p7"/>
