    <sentence id="DDI-DrugBank.d77.s16" text="The extent to which SSRI-TCA interactions may pose clinical problems will depend on the degree of inhibition, and the pharmacokinetics of the SSRI involved.">
        <entity charOffset="20-23" id="DDI-DrugBank.d77.s16.e0" text="SSRI" type="group"/>
        <entity charOffset="25-27" id="DDI-DrugBank.d77.s16.e1" text="TCA" type="group"/>
