    </sentence>
    <sentence id="DDI-DrugBank.d334.s20" text="It is recommended that serum lithium levels be monitored frequently if PRINIVIL is administered concomitantly with lithium.">
        <entity charOffset="29-35" id="DDI-DrugBank.d334.s20.e0" text="lithium" type="drug"/>
        <entity charOffset="71-78" id="DDI-DrugBank.d334.s20.e1" text="PRINIVIL" type="brand"/>
