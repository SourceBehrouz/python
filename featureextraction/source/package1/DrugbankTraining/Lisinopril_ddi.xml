        <pair ddi="false" e1="DDI-DrugBank.d334.s20.e0" e2="DDI-DrugBank.d334.s20.e2" id="DDI-DrugBank.d334.s20.p1"/>
        <pair ddi="true" e1="DDI-DrugBank.d334.s20.e1" e2="DDI-DrugBank.d334.s20.e2" id="DDI-DrugBank.d334.s20.p2" type="advise"/>
