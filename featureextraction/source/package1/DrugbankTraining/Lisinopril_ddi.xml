        <pair ddi="false" e1="DDI-DrugBank.d334.s18.e1" e2="DDI-DrugBank.d334.s18.e3" id="DDI-DrugBank.d334.s18.p4"/>
        <pair ddi="true" e1="DDI-DrugBank.d334.s18.e2" e2="DDI-DrugBank.d334.s18.e3" id="DDI-DrugBank.d334.s18.p5" type="effect"/>
    </sentence>
    <sentence id="DDI-DrugBank.d334.s19" text="Lithium toxicity was usually reversible upon discontinuation of lithium and the ACE inhibitor.">
        <entity charOffset="0-6" id="DDI-DrugBank.d334.s19.e0" text="Lithium" type="drug"/>
        <entity charOffset="64-70" id="DDI-DrugBank.d334.s19.e1" text="lithium" type="drug"/>
        <entity charOffset="80-92" id="DDI-DrugBank.d334.s19.e2" text="ACE inhibitor" type="group"/>
