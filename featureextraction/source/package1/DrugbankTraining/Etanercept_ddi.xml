<?xml version="1.0" encoding="UTF-8" standalone="no"?><document id="DDI-DrugBank.d341">
    <sentence id="DDI-DrugBank.d341.s0" text="Specific drug interaction studies have not been conducted with ENBREL .">
        <entity charOffset="63-68" id="DDI-DrugBank.d341.s0.e0" text="ENBREL" type="brand"/>
    <negationtags>DDI-DrugBank.d341.s0</negationtags></sentence>
    <sentence id="DDI-DrugBank.d341.s1" text="However, it was observed that the pharmacokinetics of ENBREL  was unaltered by concomitant methotrexate in rheumatoid arthritis patients.">
        <entity charOffset="54-59" id="DDI-DrugBank.d341.s1.e0" text="ENBREL" type="brand"/>
        <entity charOffset="91-102" id="DDI-DrugBank.d341.s1.e1" text="methotrexate" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d341.s1.e0" e2="DDI-DrugBank.d341.s1.e1" id="DDI-DrugBank.d341.s1.p0"/>
    </sentence>
    <sentence id="DDI-DrugBank.d341.s2" text="In a study in which patients with active RA were treated for up to 24 weeks with concurrent ENBREL  and anakinra therapy, a 7% rate of serious infections was observed, which was higher than that observed with ENBREL  alone (0%).">
        <entity charOffset="92-97" id="DDI-DrugBank.d341.s2.e0" text="ENBREL" type="brand"/>
        <entity charOffset="104-111" id="DDI-DrugBank.d341.s2.e1" text="anakinra" type="drug"/>
        <entity charOffset="209-214" id="DDI-DrugBank.d341.s2.e2" text="ENBREL" type="brand"/>
