        <pair ddi="false" e1="DDI-DrugBank.d341.s2.e0" e2="DDI-DrugBank.d341.s2.e2" id="DDI-DrugBank.d341.s2.p1"/>
        <pair ddi="false" e1="DDI-DrugBank.d341.s2.e1" e2="DDI-DrugBank.d341.s2.e2" id="DDI-DrugBank.d341.s2.p2"/>
    </sentence>
    <sentence id="DDI-DrugBank.d341.s3" text="Two percent of patients treated concurrently with ENBREL  and anakinra developed neutropenia (ANC   1 x 109/L).">
        <entity charOffset="50-55" id="DDI-DrugBank.d341.s3.e0" text="ENBREL" type="brand"/>
        <entity charOffset="62-69" id="DDI-DrugBank.d341.s3.e1" text="anakinra" type="drug"/>
