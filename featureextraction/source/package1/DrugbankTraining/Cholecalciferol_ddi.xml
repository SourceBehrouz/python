    </sentence>
    <sentence id="DDI-DrugBank.d404.s4" text="Thiazides: Thiazides are known to induce hypercalcemia by the reduction of calcium excretion in urine.">
        <entity charOffset="0-8" id="DDI-DrugBank.d404.s4.e0" text="Thiazides" type="group"/>
        <entity charOffset="11-19" id="DDI-DrugBank.d404.s4.e1" text="Thiazides" type="group"/>
