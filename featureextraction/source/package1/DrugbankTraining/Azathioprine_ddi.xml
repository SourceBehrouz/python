    <sentence id="DDI-DrugBank.d233.s1" text="Patients receiving azathioprine and allopurinol concomitantly should have a dose reduction of azathioprine, to approximately 1/3 to 1/4 the usual dose.">
        <entity charOffset="19-30" id="DDI-DrugBank.d233.s1.e0" text="azathioprine" type="drug"/>
