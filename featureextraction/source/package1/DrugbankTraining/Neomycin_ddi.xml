        <pair ddi="false" e1="DDI-DrugBank.d330.s1.e1" e2="DDI-DrugBank.d330.s1.e2" id="DDI-DrugBank.d330.s1.p2"/>
    </sentence>
    <sentence id="DDI-DrugBank.d330.s2" text="Oral neomycin inhibits the gastrointestinal absorption of penicillin V, oral vitamin B-12, methotrexate and 5-fluorouracil.">
        <entity charOffset="5-12" id="DDI-DrugBank.d330.s2.e0" text="neomycin" type="drug"/>
        <entity charOffset="58-69" id="DDI-DrugBank.d330.s2.e1" text="penicillin V" type="drug"/>
        <entity charOffset="77-88" id="DDI-DrugBank.d330.s2.e2" text="vitamin B-12" type="drug"/>
        <entity charOffset="91-102" id="DDI-DrugBank.d330.s2.e3" text="methotrexate" type="drug"/>
        <entity charOffset="108-121" id="DDI-DrugBank.d330.s2.e4" text="5-fluorouracil" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d330.s2.e0" e2="DDI-DrugBank.d330.s2.e1" id="DDI-DrugBank.d330.s2.p0" type="mechanism"/>
        <pair ddi="true" e1="DDI-DrugBank.d330.s2.e0" e2="DDI-DrugBank.d330.s2.e2" id="DDI-DrugBank.d330.s2.p1" type="mechanism"/>
        <pair ddi="true" e1="DDI-DrugBank.d330.s2.e0" e2="DDI-DrugBank.d330.s2.e3" id="DDI-DrugBank.d330.s2.p2" type="mechanism"/>
        <pair ddi="true" e1="DDI-DrugBank.d330.s2.e0" e2="DDI-DrugBank.d330.s2.e4" id="DDI-DrugBank.d330.s2.p3" type="mechanism"/>
        <pair ddi="false" e1="DDI-DrugBank.d330.s2.e1" e2="DDI-DrugBank.d330.s2.e2" id="DDI-DrugBank.d330.s2.p4"/>
        <pair ddi="false" e1="DDI-DrugBank.d330.s2.e1" e2="DDI-DrugBank.d330.s2.e3" id="DDI-DrugBank.d330.s2.p5"/>
