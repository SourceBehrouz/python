    <sentence id="DDI-DrugBank.d330.s0" text="Caution should be taken in concurrent or serial use of other neurotoxic and/ or nephrotoxic drugs because of possible enhancement of the nephrotoxicity and/or ototoxicity of neomycin.">
        <entity charOffset="174-181" id="DDI-DrugBank.d330.s0.e0" text="neomycin" type="drug"/>
    </sentence>
    <sentence id="DDI-DrugBank.d330.s1" text="Caution should also be taken in concurrent or serial use of other aminoglycosides and polymyxins because they may enhance neomycin s nephrotoxicity and/or ototoxicity and potentiate neomycin sulfate neuromuscular blocking effects.">
        <entity charOffset="66-80" id="DDI-DrugBank.d330.s1.e0" text="aminoglycosides" type="group"/>
        <entity charOffset="86-95" id="DDI-DrugBank.d330.s1.e1" text="polymyxins" type="group"/>
        <entity charOffset="182-197" id="DDI-DrugBank.d330.s1.e2" text="neomycin sulfate" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d330.s1.e0" e2="DDI-DrugBank.d330.s1.e1" id="DDI-DrugBank.d330.s1.p0"/>
