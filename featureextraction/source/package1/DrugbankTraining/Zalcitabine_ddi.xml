        <entity charOffset="54-56" id="DDI-DrugBank.d263.s10.e3" text="ZDV" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d263.s10.e0" e2="DDI-DrugBank.d263.s10.e1" id="DDI-DrugBank.d263.s10.p0"/>
