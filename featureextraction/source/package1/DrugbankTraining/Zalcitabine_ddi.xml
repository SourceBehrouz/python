    <sentence id="DDI-DrugBank.d263.s11" text="Pharmacokinetic data suggest that absorption, metabolism, and elimination of each of these drugs are unchanged when they are used together."/>
    <sentence id="DDI-DrugBank.d263.s12" text="Drugs Associated With Peripheral Neuropathy: The concomitant use of HIVID with drugs that have the potential to cause peripheral neuropathy should be avoided where possible.">
        <entity charOffset="68-72" id="DDI-DrugBank.d263.s12.e0" text="HIVID" type="drug"/>
    </sentence>
