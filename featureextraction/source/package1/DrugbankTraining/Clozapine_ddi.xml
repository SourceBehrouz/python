    </sentence>
    <sentence id="DDI-DrugBank.d480.s16" text="Concomitant administration of drugs known to inhibit the activity of cytochrome P450 isozymes may increase the plasma levels of clozapine.">
        <entity charOffset="128-136" id="DDI-DrugBank.d480.s16.e0" text="clozapine" type="drug"/>
    </sentence>
    <sentence id="DDI-DrugBank.d480.s17" text="Cimetidine, caffeine, and erythromycin may increase plasma levels of Clozapine, potentially resulting in adverse effects.">
        <entity charOffset="0-9" id="DDI-DrugBank.d480.s17.e0" text="Cimetidine" type="drug"/>
        <entity charOffset="12-19" id="DDI-DrugBank.d480.s17.e1" text="caffeine" type="drug"/>
        <entity charOffset="26-37" id="DDI-DrugBank.d480.s17.e2" text="erythromycin" type="drug"/>
        <entity charOffset="69-77" id="DDI-DrugBank.d480.s17.e3" text="Clozapine" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d480.s17.e0" e2="DDI-DrugBank.d480.s17.e1" id="DDI-DrugBank.d480.s17.p0"/>
