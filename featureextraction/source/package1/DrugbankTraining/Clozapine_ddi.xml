        <entity charOffset="139-147" id="DDI-DrugBank.d480.s15.e4" text="Clozapine" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d480.s15.e0" e2="DDI-DrugBank.d480.s15.e1" id="DDI-DrugBank.d480.s15.p0"/>
