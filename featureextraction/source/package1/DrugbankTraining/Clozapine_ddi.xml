        <pair ddi="true" e1="DDI-DrugBank.d480.s22.e1" e2="DDI-DrugBank.d480.s22.e2" id="DDI-DrugBank.d480.s22.p4" type="mechanism"/>
        <pair ddi="true" e1="DDI-DrugBank.d480.s22.e1" e2="DDI-DrugBank.d480.s22.e3" id="DDI-DrugBank.d480.s22.p5" type="mechanism"/>
        <pair ddi="true" e1="DDI-DrugBank.d480.s22.e1" e2="DDI-DrugBank.d480.s22.e4" id="DDI-DrugBank.d480.s22.p6" type="mechanism"/>
        <pair ddi="false" e1="DDI-DrugBank.d480.s22.e2" e2="DDI-DrugBank.d480.s22.e3" id="DDI-DrugBank.d480.s22.p7"/>
        <pair ddi="false" e1="DDI-DrugBank.d480.s22.e2" e2="DDI-DrugBank.d480.s22.e4" id="DDI-DrugBank.d480.s22.p8"/>
