    <sentence id="DDI-DrugBank.d480.s9" text="Clozapine may potentiate the hypotensive effects of antihypertensive drugs and the anticholinergic effects of atropine-type drugs.">
        <entity charOffset="0-8" id="DDI-DrugBank.d480.s9.e0" text="Clozapine" type="drug"/>
        <entity charOffset="52-73" id="DDI-DrugBank.d480.s9.e1" text="antihypertensive drugs" type="group"/>
        <entity charOffset="110-117" id="DDI-DrugBank.d480.s9.e2" text="atropine" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d480.s9.e0" e2="DDI-DrugBank.d480.s9.e1" id="DDI-DrugBank.d480.s9.p0" type="effect"/>
        <pair ddi="true" e1="DDI-DrugBank.d480.s9.e0" e2="DDI-DrugBank.d480.s9.e2" id="DDI-DrugBank.d480.s9.p1" type="effect"/>
