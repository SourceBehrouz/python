        <pair ddi="false" e1="DDI-DrugBank.d480.s20.e2" e2="DDI-DrugBank.d480.s20.e3" id="DDI-DrugBank.d480.s20.p5"/>
    </sentence>
    <sentence id="DDI-DrugBank.d480.s21" text="Paroxetine produced only minor changes in the levels of clozapine and its metabolites.">
        <entity charOffset="0-9" id="DDI-DrugBank.d480.s21.e0" text="Paroxetine" type="drug"/>
        <entity charOffset="56-64" id="DDI-DrugBank.d480.s21.e1" text="clozapine" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d480.s21.e0" e2="DDI-DrugBank.d480.s21.e1" id="DDI-DrugBank.d480.s21.p0" type="mechanism"/>
    </sentence>
    <sentence id="DDI-DrugBank.d480.s22" text="However, other published reports describe modest elevations (less than two-fold) of clozapine and metabolite concentrations when clozapine was taken with paroxetine, fluoxetine, and sertraline.">
        <entity charOffset="84-92" id="DDI-DrugBank.d480.s22.e0" text="clozapine" type="drug"/>
        <entity charOffset="129-137" id="DDI-DrugBank.d480.s22.e1" text="clozapine" type="drug"/>
        <entity charOffset="154-163" id="DDI-DrugBank.d480.s22.e2" text="paroxetine" type="drug"/>
        <entity charOffset="166-175" id="DDI-DrugBank.d480.s22.e3" text="fluoxetine" type="drug"/>
        <entity charOffset="182-191" id="DDI-DrugBank.d480.s22.e4" text="sertraline" type="drug"/>
