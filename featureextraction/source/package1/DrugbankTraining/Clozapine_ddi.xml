        <pair ddi="true" e1="DDI-DrugBank.d480.s18.e0" e2="DDI-DrugBank.d480.s18.e1" id="DDI-DrugBank.d480.s18.p0" type="advise"/>
        <pair ddi="false" e1="DDI-DrugBank.d480.s18.e0" e2="DDI-DrugBank.d480.s18.e2" id="DDI-DrugBank.d480.s18.p1"/>
