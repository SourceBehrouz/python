    </sentence>
    <sentence id="DDI-DrugBank.d543.s3" text="Anticholinergic drugs in the presence of increased intraocular pressure may be hazardous when taken concurrently with agents such as corti costeroids..">
        <entity charOffset="0-20" id="DDI-DrugBank.d543.s3.e0" text="Anticholinergic drugs" type="group"/>
    </sentence>
    <sentence id="DDI-DrugBank.d543.s4" text="Anticholinergic agents may affect gastrointestinal absorption of various drugs, such as slowly dissolving dosage forms of digoxin;">
        <entity charOffset="0-21" id="DDI-DrugBank.d543.s4.e0" text="Anticholinergic agents" type="group"/>
        <entity charOffset="122-128" id="DDI-DrugBank.d543.s4.e1" text="digoxin" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d543.s4.e0" e2="DDI-DrugBank.d543.s4.e1" id="DDI-DrugBank.d543.s4.p0" type="mechanism"/>
    </sentence>
    <sentence id="DDI-DrugBank.d543.s5" text="increased serum digoxin concentrations may result.">
        <entity charOffset="16-22" id="DDI-DrugBank.d543.s5.e0" text="digoxin" type="drug"/>
    </sentence>
    <sentence id="DDI-DrugBank.d543.s6" text="Anticholinergic drugs may antagonize the effects of the drugs that alter gastrointestinal motility, such as metoclopramide.">
        <entity charOffset="0-20" id="DDI-DrugBank.d543.s6.e0" text="Anticholinergic drugs" type="group"/>
        <entity charOffset="108-121" id="DDI-DrugBank.d543.s6.e1" text="metoclopramide" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d543.s6.e0" e2="DDI-DrugBank.d543.s6.e1" id="DDI-DrugBank.d543.s6.p0" type="mechanism"/>
    </sentence>
    <sentence id="DDI-DrugBank.d543.s7" text="Because antacids may interfere with the absorption of anticholinergic agents, simultaneous use of these drugs should be avoided.">
        <entity charOffset="8-15" id="DDI-DrugBank.d543.s7.e0" text="antacids" type="group"/>
        <entity charOffset="54-75" id="DDI-DrugBank.d543.s7.e1" text="anticholinergic agents" type="group"/>
        <pair ddi="true" e1="DDI-DrugBank.d543.s7.e0" e2="DDI-DrugBank.d543.s7.e1" id="DDI-DrugBank.d543.s7.p0" type="mechanism"/>
    </sentence>
    <sentence id="DDI-DrugBank.d543.s8" text="The inhibiting effects of anticholinergic drugs on gastric hydrochloric acid secretion are antagonized by agents used to treat achlorhydria and those used to test gastric secretion.">
        <entity charOffset="26-46" id="DDI-DrugBank.d543.s8.e0" text="anticholinergic drugs" type="group"/>
    </sentence>
