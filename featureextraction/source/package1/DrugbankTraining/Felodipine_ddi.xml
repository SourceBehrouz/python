    </sentence>
    <sentence id="DDI-DrugBank.d316.s15" text="In such patients, the mean area under the felodipine plasma concentration-time curve was also reduced to approximately 6% of that observed in healthy volunteers.">
        <entity charOffset="42-51" id="DDI-DrugBank.d316.s15.e0" text="felodipine" type="drug"/>
    </sentence>
    <sentence id="DDI-DrugBank.d316.s16" text="Since a clinically significant interaction may be anticipated, alternative antihypertensive therapy should be considered in these patients.">
        <entity charOffset="75-90" id="DDI-DrugBank.d316.s16.e0" text="antihypertensive" type="drug"/>
    </sentence>
    <sentence id="DDI-DrugBank.d316.s17" text="Tacrolimus  Felodipine may increase the blood concentration of tacrolimus.">
        <entity charOffset="12-21" id="DDI-DrugBank.d316.s17.e0" text="Felodipine" type="drug"/>
        <entity charOffset="63-72" id="DDI-DrugBank.d316.s17.e1" text="tacrolimus" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d316.s17.e0" e2="DDI-DrugBank.d316.s17.e1" id="DDI-DrugBank.d316.s17.p0"/>
    </sentence>
    <sentence id="DDI-DrugBank.d316.s18" text="When given concomitantly with felodipine, the tacrolimus blood concentration should be followed and the tacrolimus dose may need to be adjusted.">
        <entity charOffset="30-39" id="DDI-DrugBank.d316.s18.e0" text="felodipine" type="drug"/>
        <entity charOffset="46-55" id="DDI-DrugBank.d316.s18.e1" text="tacrolimus" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d316.s18.e0" e2="DDI-DrugBank.d316.s18.e1" id="DDI-DrugBank.d316.s18.p0"/>
    </sentence>
    <sentence id="DDI-DrugBank.d316.s19" text="Other Concomitant Therapy  In healthy subjects there were no clinically significant interactions when felodipine was given concomitantly with indomethacin or spironolactone.">
        <entity charOffset="102-111" id="DDI-DrugBank.d316.s19.e0" text="felodipine" type="drug"/>
        <entity charOffset="142-153" id="DDI-DrugBank.d316.s19.e1" text="indomethacin" type="drug"/>
        <entity charOffset="158-171" id="DDI-DrugBank.d316.s19.e2" text="spironolactone" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d316.s19.e0" e2="DDI-DrugBank.d316.s19.e1" id="DDI-DrugBank.d316.s19.p0"/>
