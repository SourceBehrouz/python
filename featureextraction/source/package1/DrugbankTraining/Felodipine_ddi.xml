    <sentence id="DDI-DrugBank.d316.s0" text="CYP3A4 Inhibitors Felodipine is metabolized by CYP3A4.">
        <entity charOffset="0-5" id="DDI-DrugBank.d316.s0.e0" text="CYP3A4" type="drug"/>
        <entity charOffset="47-52" id="DDI-DrugBank.d316.s0.e1" text="CYP3A4" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d316.s0.e0" e2="DDI-DrugBank.d316.s0.e1" id="DDI-DrugBank.d316.s0.p0"/>
    </sentence>
    <sentence id="DDI-DrugBank.d316.s1" text="Co-administration of CYP3A4 inhibitors (eg, ketoconazole, itraconazole, erythromycin, grapefruit juice, cimetidine) with felodipine may lead to several- fold increases in the plasma levels of felodipine, either due to an increase in bioavailability or due to a decrease in metabolism.">
        <entity charOffset="44-55" id="DDI-DrugBank.d316.s1.e0" text="ketoconazole" type="drug"/>
        <entity charOffset="58-69" id="DDI-DrugBank.d316.s1.e1" text="itraconazole" type="drug"/>
        <entity charOffset="72-83" id="DDI-DrugBank.d316.s1.e2" text="erythromycin" type="drug"/>
        <entity charOffset="104-113" id="DDI-DrugBank.d316.s1.e3" text="cimetidine" type="drug"/>
        <entity charOffset="121-130" id="DDI-DrugBank.d316.s1.e4" text="felodipine" type="drug"/>
        <entity charOffset="192-201" id="DDI-DrugBank.d316.s1.e5" text="felodipine" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d316.s1.e0" e2="DDI-DrugBank.d316.s1.e1" id="DDI-DrugBank.d316.s1.p0"/>
