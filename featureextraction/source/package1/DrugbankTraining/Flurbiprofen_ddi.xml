    </sentence>
    <sentence id="DDI-DrugBank.d529.s6" text="Concurrent use of flurbiprofen and aspirin is therefore not recommended.">
        <entity charOffset="18-29" id="DDI-DrugBank.d529.s6.e0" text="flurbiprofen" type="drug"/>
        <entity charOffset="35-41" id="DDI-DrugBank.d529.s6.e1" text="aspirin" type="brand"/>
