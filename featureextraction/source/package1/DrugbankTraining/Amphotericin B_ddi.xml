        <pair ddi="true" e1="DDI-DrugBank.d318.s2.e2" e2="DDI-DrugBank.d318.s2.e3" id="DDI-DrugBank.d318.s2.p5" type="effect"/>
    </sentence>
    <sentence id="DDI-DrugBank.d318.s3" text="Avoid concomitant use unless necessary to control side effects of amphotericin B.">
        <entity charOffset="66-79" id="DDI-DrugBank.d318.s3.e0" text="amphotericin B" type="drug"/>
    </sentence>
    <sentence id="DDI-DrugBank.d318.s4" text="If used concomitantly, closely monitor serum electrolytes and cardiac function."/>
    <sentence id="DDI-DrugBank.d318.s5" text="Digitalis glycosides: amphotericin B-induced hypokalemia may potentiate digitalis toxicity.">
        <entity charOffset="0-19" id="DDI-DrugBank.d318.s5.e0" text="Digitalis glycosides" type="group"/>
        <entity charOffset="22-35" id="DDI-DrugBank.d318.s5.e1" text="amphotericin B" type="drug"/>
        <entity charOffset="72-80" id="DDI-DrugBank.d318.s5.e2" text="digitalis" type="group"/>
