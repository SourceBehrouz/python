<?xml version="1.0" encoding="UTF-8" standalone="no"?><document id="DDI-DrugBank.d318">
    <sentence id="DDI-DrugBank.d318.s0" text="When administered concurrently, the following drugs may interact with amphotericin B: Antineoplastic agents: may enhance the potential for renal toxicity, bronchospasm and hypotension.">
        <entity charOffset="70-83" id="DDI-DrugBank.d318.s0.e0" text="amphotericin B" type="drug"/>
        <entity charOffset="86-106" id="DDI-DrugBank.d318.s0.e1" text="Antineoplastic agents" type="group"/>
        <pair ddi="true" e1="DDI-DrugBank.d318.s0.e0" e2="DDI-DrugBank.d318.s0.e1" id="DDI-DrugBank.d318.s0.p0" type="effect"/>
    </sentence>
    <sentence id="DDI-DrugBank.d318.s1" text="Antineoplastic agents (e. g., nitrogen mustard, etc.) should be given concomitantly only with great caution.">
        <entity charOffset="0-20" id="DDI-DrugBank.d318.s1.e0" text="Antineoplastic agents" type="group"/>
        <entity charOffset="30-45" id="DDI-DrugBank.d318.s1.e1" text="nitrogen mustard" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d318.s1.e0" e2="DDI-DrugBank.d318.s1.e1" id="DDI-DrugBank.d318.s1.p0"/>
    </sentence>
    <sentence id="DDI-DrugBank.d318.s2" text="Corticosteroids and Corticotropin (ACTH): may potentiate amphotericin B- induced hypokalemia which may predispose the patient to cardiac dysfunction.">
        <entity charOffset="0-14" id="DDI-DrugBank.d318.s2.e0" text="Corticosteroids" type="group"/>
        <entity charOffset="20-32" id="DDI-DrugBank.d318.s2.e1" text="Corticotropin" type="drug"/>
        <entity charOffset="35-38" id="DDI-DrugBank.d318.s2.e2" text="ACTH" type="group"/>
        <entity charOffset="57-70" id="DDI-DrugBank.d318.s2.e3" text="amphotericin B" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d318.s2.e0" e2="DDI-DrugBank.d318.s2.e1" id="DDI-DrugBank.d318.s2.p0"/>
        <pair ddi="false" e1="DDI-DrugBank.d318.s2.e0" e2="DDI-DrugBank.d318.s2.e2" id="DDI-DrugBank.d318.s2.p1"/>
        <pair ddi="true" e1="DDI-DrugBank.d318.s2.e0" e2="DDI-DrugBank.d318.s2.e3" id="DDI-DrugBank.d318.s2.p2" type="effect"/>
        <pair ddi="false" e1="DDI-DrugBank.d318.s2.e1" e2="DDI-DrugBank.d318.s2.e2" id="DDI-DrugBank.d318.s2.p3"/>
