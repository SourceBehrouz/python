    <sentence id="DDI-DrugBank.d253.s0" text="Beta-adrenergic blocking agents: concurrent use may blunt the response to arbutamine.">
        <entity charOffset="0-30" id="DDI-DrugBank.d253.s0.e0" text="Beta-adrenergic blocking agents" type="group"/>
