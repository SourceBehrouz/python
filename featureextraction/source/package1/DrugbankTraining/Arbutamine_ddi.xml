    <sentence id="DDI-DrugBank.d253.s1" text="Beta-adrenergic blocking agents should be withdrawn at least 48 hours before conducting an arbutamine-mediated stress test.">
        <entity charOffset="0-30" id="DDI-DrugBank.d253.s1.e0" text="Beta-adrenergic blocking agents" type="group"/>
