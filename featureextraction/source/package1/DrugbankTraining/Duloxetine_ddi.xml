    <sentence id="DDI-DrugBank.d548.s0" text="Potential for Other Drugs to Affect Duloxetine: Both CYP1A2 and CYP2D6 are responsible for duloxetine metabolism.">
        <entity charOffset="36-45" id="DDI-DrugBank.d548.s0.e0" text="Duloxetine" type="drug"/>
        <entity charOffset="91-100" id="DDI-DrugBank.d548.s0.e1" text="duloxetine" type="drug"/>
