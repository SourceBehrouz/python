    </sentence>
    <sentence id="DDI-DrugBank.d99.s8" text="While all the selective serotonin reuptake inhibitors (SSRIs), e.g., fluoxetine, sertraline, and paroxetine, inhibit P450 2D6, they may vary in the extent of inhibition.">
        <entity charOffset="24-52" id="DDI-DrugBank.d99.s8.e0" text="serotonin reuptake inhibitors" type="group"/>
        <entity charOffset="55-59" id="DDI-DrugBank.d99.s8.e1" text="SSRIs" type="group"/>
        <entity charOffset="69-78" id="DDI-DrugBank.d99.s8.e2" text="fluoxetine" type="drug"/>
        <entity charOffset="81-90" id="DDI-DrugBank.d99.s8.e3" text="sertraline" type="drug"/>
        <entity charOffset="97-106" id="DDI-DrugBank.d99.s8.e4" text="paroxetine" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d99.s8.e0" e2="DDI-DrugBank.d99.s8.e1" id="DDI-DrugBank.d99.s8.p0"/>
        <pair ddi="false" e1="DDI-DrugBank.d99.s8.e0" e2="DDI-DrugBank.d99.s8.e2" id="DDI-DrugBank.d99.s8.p1"/>
