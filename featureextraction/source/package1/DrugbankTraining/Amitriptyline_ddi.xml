    <sentence id="DDI-DrugBank.d99.s19" text="Hyperpyrexia has been reported when amitriptyline HCl is administered with anticholinergic agents or with neuroleptic drugs, particularly during hot weather.">
        <entity charOffset="36-52" id="DDI-DrugBank.d99.s19.e0" text="amitriptyline HCl" type="drug"/>
        <entity charOffset="75-89" id="DDI-DrugBank.d99.s19.e1" text="anticholinergic" type="drug"/>
        <entity charOffset="106-116" id="DDI-DrugBank.d99.s19.e2" text="neuroleptic" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d99.s19.e0" e2="DDI-DrugBank.d99.s19.e1" id="DDI-DrugBank.d99.s19.p0" type="effect"/>
        <pair ddi="false" e1="DDI-DrugBank.d99.s19.e0" e2="DDI-DrugBank.d99.s19.e2" id="DDI-DrugBank.d99.s19.p1"/>
        <pair ddi="false" e1="DDI-DrugBank.d99.s19.e1" e2="DDI-DrugBank.d99.s19.e2" id="DDI-DrugBank.d99.s19.p2"/>
    </sentence>
    <sentence id="DDI-DrugBank.d99.s20" text="Paralytic ileus may occur in patients taking tricyclic antidepressants in combination with anticholinergic-type drugs.">
        <entity charOffset="45-69" id="DDI-DrugBank.d99.s20.e0" text="tricyclic antidepressants" type="drug"/>
        <entity charOffset="91-105" id="DDI-DrugBank.d99.s20.e1" text="anticholinergic" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d99.s20.e0" e2="DDI-DrugBank.d99.s20.e1" id="DDI-DrugBank.d99.s20.p0" type="effect"/>
    </sentence>
    <sentence id="DDI-DrugBank.d99.s21" text="Cimetidine is reported to reduce hepatic metabolism of certain tricyclic antidepressants, thereby delaying elimination and increasing steady-state concentrations of these drugs.">
        <entity charOffset="0-9" id="DDI-DrugBank.d99.s21.e0" text="Cimetidine" type="drug"/>
        <entity charOffset="63-87" id="DDI-DrugBank.d99.s21.e1" text="tricyclic antidepressants" type="drug"/>
        <entity charOffset="171-175" id="DDI-DrugBank.d99.s21.e2" text="drugs" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d99.s21.e0" e2="DDI-DrugBank.d99.s21.e1" id="DDI-DrugBank.d99.s21.p0" type="mechanism"/>
        <pair ddi="false" e1="DDI-DrugBank.d99.s21.e0" e2="DDI-DrugBank.d99.s21.e2" id="DDI-DrugBank.d99.s21.p1"/>
        <pair ddi="false" e1="DDI-DrugBank.d99.s21.e1" e2="DDI-DrugBank.d99.s21.e2" id="DDI-DrugBank.d99.s21.p2"/>
    </sentence>
    <sentence id="DDI-DrugBank.d99.s22" text="Clinically significant effects have been reported with the tricyclic antidepressants when used concomitantly with cimetidine.">
        <entity charOffset="59-83" id="DDI-DrugBank.d99.s22.e0" text="tricyclic antidepressants" type="drug"/>
        <entity charOffset="114-123" id="DDI-DrugBank.d99.s22.e1" text="cimetidine" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d99.s22.e0" e2="DDI-DrugBank.d99.s22.e1" id="DDI-DrugBank.d99.s22.p0" type="effect"/>
    </sentence>
    <sentence id="DDI-DrugBank.d99.s23" text="Increases in plasma levels of tricyclic antidepressants, and in the frequency and severity of side effects, particularly anticholinergic, have been reported when cimetidine was added to the drug regimen.">
        <entity charOffset="30-54" id="DDI-DrugBank.d99.s23.e0" text="tricyclic antidepressants" type="drug"/>
        <entity charOffset="121-135" id="DDI-DrugBank.d99.s23.e1" text="anticholinergic" type="drug"/>
        <entity charOffset="162-171" id="DDI-DrugBank.d99.s23.e2" text="cimetidine" type="drug"/>
        <entity charOffset="190-193" id="DDI-DrugBank.d99.s23.e3" text="drug" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d99.s23.e0" e2="DDI-DrugBank.d99.s23.e1" id="DDI-DrugBank.d99.s23.p0"/>
        <pair ddi="true" e1="DDI-DrugBank.d99.s23.e0" e2="DDI-DrugBank.d99.s23.e2" id="DDI-DrugBank.d99.s23.p1" type="mechanism"/>
        <pair ddi="false" e1="DDI-DrugBank.d99.s23.e0" e2="DDI-DrugBank.d99.s23.e3" id="DDI-DrugBank.d99.s23.p2"/>
        <pair ddi="false" e1="DDI-DrugBank.d99.s23.e1" e2="DDI-DrugBank.d99.s23.e2" id="DDI-DrugBank.d99.s23.p3"/>
        <pair ddi="false" e1="DDI-DrugBank.d99.s23.e1" e2="DDI-DrugBank.d99.s23.e3" id="DDI-DrugBank.d99.s23.p4"/>
        <pair ddi="false" e1="DDI-DrugBank.d99.s23.e2" e2="DDI-DrugBank.d99.s23.e3" id="DDI-DrugBank.d99.s23.p5"/>
    </sentence>
    <sentence id="DDI-DrugBank.d99.s24" text="Discontinuation of cimetidine in well-controlled patients receiving tricyclic antidepressants and cimetidine may decrease the plasma levels and efficacy of the antidepressants.">
