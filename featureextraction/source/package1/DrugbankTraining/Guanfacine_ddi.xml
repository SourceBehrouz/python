    <sentence id="DDI-DrugBank.d507.s6" text="The beta-blocker should be withdrawn first.">
        <entity charOffset="4-15" id="DDI-DrugBank.d507.s6.e0" text="beta-blocker" type="group"/>
    </sentence>
    <sentence id="DDI-DrugBank.d507.s7" text="The gradual withdrawal of guafacine or a cardioselective beta-blocker could be substituted.">
