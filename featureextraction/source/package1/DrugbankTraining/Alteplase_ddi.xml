    <sentence id="DDI-DrugBank.d508.s3" text="Because heparin, aspirin, or Activase may cause bleeding complications, careful monitoring for bleeding is advised, especially at arterial puncture sites.">
        <entity charOffset="8-14" id="DDI-DrugBank.d508.s3.e0" text="heparin" type="drug"/>
