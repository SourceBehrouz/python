    <sentence id="DDI-DrugBank.d72.s2" text="Phenytoin increases the clearance of busulfan by 15% or more, possibly due to the induction of glutathione-S-transferase.">
        <entity charOffset="0-8" id="DDI-DrugBank.d72.s2.e0" text="Phenytoin" type="drug"/>
        <entity charOffset="37-44" id="DDI-DrugBank.d72.s2.e1" text="busulfan" type="drug"/>
