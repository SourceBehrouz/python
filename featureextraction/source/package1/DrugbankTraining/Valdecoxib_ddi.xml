    </sentence>
    <sentence id="DDI-DrugBank.d328.s14" text="Valdecoxib did not have a statistically significant effect on the pharmacokinetics of phenytoin (a CYP 2C9 and CYP 2C19 substrate).">
        <entity charOffset="0-9" id="DDI-DrugBank.d328.s14.e0" text="Valdecoxib" type="drug"/>
        <entity charOffset="86-94" id="DDI-DrugBank.d328.s14.e1" text="phenytoin" type="drug"/>
