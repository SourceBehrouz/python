    <sentence id="DDI-DrugBank.d328.s0" text="The drug interaction studies with valdecoxib were performed both with valdecoxib and a rapidly hydrolyzed intravenous prodrug form.">
        <entity charOffset="34-43" id="DDI-DrugBank.d328.s0.e0" text="valdecoxib" type="drug"/>
        <entity charOffset="70-79" id="DDI-DrugBank.d328.s0.e1" text="valdecoxib" type="drug"/>
