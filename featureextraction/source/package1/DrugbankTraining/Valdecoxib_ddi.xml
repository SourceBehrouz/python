    </sentence>
    <sentence id="DDI-DrugBank.d328.s13" text="Patients already stabilized on valdecoxib should be closely monitored for loss of symptom control with phenytoin coadministration.">
        <entity charOffset="31-40" id="DDI-DrugBank.d328.s13.e0" text="valdecoxib" type="drug"/>
        <entity charOffset="103-111" id="DDI-DrugBank.d328.s13.e1" text="phenytoin" type="drug"/>
