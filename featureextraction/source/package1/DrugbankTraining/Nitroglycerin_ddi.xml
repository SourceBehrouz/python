    <sentence id="DDI-DrugBank.d14.s0" text="The vasodilating effects of nitroglycerin may be additive with those of other vasodilators.">
        <entity charOffset="28-40" id="DDI-DrugBank.d14.s0.e0" text="nitroglycerin" type="drug"/>
        <entity charOffset="78-89" id="DDI-DrugBank.d14.s0.e1" text="vasodilators" type="group"/>
