    <sentence id="DDI-DrugBank.d217.s0" text="Elevated plasma levels of theophylline have been reported with concomitant quinolone use.">
        <entity charOffset="26-37" id="DDI-DrugBank.d217.s0.e0" text="theophylline" type="drug"/>
        <entity charOffset="75-83" id="DDI-DrugBank.d217.s0.e1" text="quinolone" type="group"/>
        <pair ddi="true" e1="DDI-DrugBank.d217.s0.e0" e2="DDI-DrugBank.d217.s0.e1" id="DDI-DrugBank.d217.s0.p0" type="mechanism"/>
    </sentence>
    <sentence id="DDI-DrugBank.d217.s1" text="There have been reports of theophylline-related side effects in patients on concomitant therapy with norfloxacin and theophylline.">
        <entity charOffset="27-38" id="DDI-DrugBank.d217.s1.e0" text="theophylline" type="drug"/>
        <entity charOffset="101-111" id="DDI-DrugBank.d217.s1.e1" text="norfloxacin" type="drug"/>
        <entity charOffset="117-128" id="DDI-DrugBank.d217.s1.e2" text="theophylline" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d217.s1.e0" e2="DDI-DrugBank.d217.s1.e1" id="DDI-DrugBank.d217.s1.p0"/>
        <pair ddi="false" e1="DDI-DrugBank.d217.s1.e0" e2="DDI-DrugBank.d217.s1.e2" id="DDI-DrugBank.d217.s1.p1"/>
        <pair ddi="true" e1="DDI-DrugBank.d217.s1.e1" e2="DDI-DrugBank.d217.s1.e2" id="DDI-DrugBank.d217.s1.p2" type="effect"/>
    </sentence>
    <sentence id="DDI-DrugBank.d217.s2" text="Therefore, monitoring of theophylline plasma levels should be considered and dosage of theophylline adjusted as required.">
        <entity charOffset="25-36" id="DDI-DrugBank.d217.s2.e0" text="theophylline" type="drug"/>
        <entity charOffset="87-98" id="DDI-DrugBank.d217.s2.e1" text="theophylline" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d217.s2.e0" e2="DDI-DrugBank.d217.s2.e1" id="DDI-DrugBank.d217.s2.p0"/>
    </sentence>
    <sentence id="DDI-DrugBank.d217.s3" text="Elevated serum levels of cyclosporine have been reported with concomitant use of cyclosporine with norfloxacin.">
        <entity charOffset="25-36" id="DDI-DrugBank.d217.s3.e0" text="cyclosporine" type="drug"/>
        <entity charOffset="81-92" id="DDI-DrugBank.d217.s3.e1" text="cyclosporine" type="drug"/>
        <entity charOffset="99-109" id="DDI-DrugBank.d217.s3.e2" text="norfloxacin" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d217.s3.e0" e2="DDI-DrugBank.d217.s3.e1" id="DDI-DrugBank.d217.s3.p0"/>
        <pair ddi="false" e1="DDI-DrugBank.d217.s3.e0" e2="DDI-DrugBank.d217.s3.e2" id="DDI-DrugBank.d217.s3.p1"/>
        <pair ddi="true" e1="DDI-DrugBank.d217.s3.e1" e2="DDI-DrugBank.d217.s3.e2" id="DDI-DrugBank.d217.s3.p2" type="mechanism"/>
    </sentence>
    <sentence id="DDI-DrugBank.d217.s4" text="Therefore, cyclosporine serum levels should be monitored and appropriate cyclosporine dosage adjustments made when these drugs are used concomitantly.">
        <entity charOffset="11-22" id="DDI-DrugBank.d217.s4.e0" text="cyclosporine" type="drug"/>
        <entity charOffset="73-84" id="DDI-DrugBank.d217.s4.e1" text="cyclosporine" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d217.s4.e0" e2="DDI-DrugBank.d217.s4.e1" id="DDI-DrugBank.d217.s4.p0"/>
    </sentence>
    <sentence id="DDI-DrugBank.d217.s5" text="Quinolones, including norfloxacin, may enhance the effects of oral anticoagulants, including warfarin or its derivatives or similar agents.">
        <entity charOffset="0-9" id="DDI-DrugBank.d217.s5.e0" text="Quinolones" type="group"/>
        <entity charOffset="22-32" id="DDI-DrugBank.d217.s5.e1" text="norfloxacin" type="drug"/>
        <entity charOffset="67-80" id="DDI-DrugBank.d217.s5.e2" text="anticoagulants" type="group"/>
        <entity charOffset="93-100" id="DDI-DrugBank.d217.s5.e3" text="warfarin" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d217.s5.e0" e2="DDI-DrugBank.d217.s5.e1" id="DDI-DrugBank.d217.s5.p0"/>
        <pair ddi="true" e1="DDI-DrugBank.d217.s5.e0" e2="DDI-DrugBank.d217.s5.e2" id="DDI-DrugBank.d217.s5.p1" type="effect"/>
        <pair ddi="true" e1="DDI-DrugBank.d217.s5.e0" e2="DDI-DrugBank.d217.s5.e3" id="DDI-DrugBank.d217.s5.p2" type="effect"/>
        <pair ddi="true" e1="DDI-DrugBank.d217.s5.e1" e2="DDI-DrugBank.d217.s5.e2" id="DDI-DrugBank.d217.s5.p3" type="effect"/>
        <pair ddi="true" e1="DDI-DrugBank.d217.s5.e1" e2="DDI-DrugBank.d217.s5.e3" id="DDI-DrugBank.d217.s5.p4" type="effect"/>
