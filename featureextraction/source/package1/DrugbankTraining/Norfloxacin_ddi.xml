    <sentence id="DDI-DrugBank.d217.s13" text="Some quinolones have also been shown to interfere with the metabolism of caffeine.">
        <entity charOffset="5-14" id="DDI-DrugBank.d217.s13.e0" text="quinolones" type="group"/>
        <entity charOffset="73-80" id="DDI-DrugBank.d217.s13.e1" text="caffeine" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d217.s13.e0" e2="DDI-DrugBank.d217.s13.e1" id="DDI-DrugBank.d217.s13.p0" type="mechanism"/>
    </sentence>
    <sentence id="DDI-DrugBank.d217.s14" text="This may lead to reduced clearance of caffeine and a prolongation of its plasma half-life.">
        <entity charOffset="38-45" id="DDI-DrugBank.d217.s14.e0" text="caffeine" type="drug"/>
    </sentence>
