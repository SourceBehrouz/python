    <sentence id="DDI-DrugBank.d462.s0" text="Aminoglycosides: The mixing of piperacillin with an aminoglycoside in vitro can result in substantial inactivation of the aminoglycoside.">
        <entity charOffset="0-14" id="DDI-DrugBank.d462.s0.e0" text="Aminoglycosides" type="group"/>
        <entity charOffset="31-42" id="DDI-DrugBank.d462.s0.e1" text="piperacillin" type="drug"/>
        <entity charOffset="52-65" id="DDI-DrugBank.d462.s0.e2" text="aminoglycoside" type="group"/>
        <entity charOffset="122-135" id="DDI-DrugBank.d462.s0.e3" text="aminoglycoside" type="group"/>
        <pair ddi="false" e1="DDI-DrugBank.d462.s0.e0" e2="DDI-DrugBank.d462.s0.e1" id="DDI-DrugBank.d462.s0.p0"/>
        <pair ddi="false" e1="DDI-DrugBank.d462.s0.e0" e2="DDI-DrugBank.d462.s0.e2" id="DDI-DrugBank.d462.s0.p1"/>
