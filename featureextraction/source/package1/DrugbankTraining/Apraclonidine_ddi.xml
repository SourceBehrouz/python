    </sentence>
    <sentence id="DDI-DrugBank.d224.s9" text="Patients using cardiovascular drugs concurrently with IOPIDINE 0.5% Ophthalmic Solution should have pulse and blood pressures frequently monitored.">
        <entity charOffset="54-61" id="DDI-DrugBank.d224.s9.e0" text="IOPIDINE" type="brand"/>
    </sentence>
    <sentence id="DDI-DrugBank.d224.s10" text="Caution should be exercised with simultaneous use of clonidine and other similar pharmacologic agents">
        <entity charOffset="53-61" id="DDI-DrugBank.d224.s10.e0" text="clonidine" type="drug"/>
    </sentence>
