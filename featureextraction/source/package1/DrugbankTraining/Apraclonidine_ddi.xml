    <sentence id="DDI-DrugBank.d224.s0" text="Apraclonidine should not be used in patients receiving MAO inhibitors..">
        <entity charOffset="0-12" id="DDI-DrugBank.d224.s0.e0" text="Apraclonidine" type="drug"/>
        <entity charOffset="55-68" id="DDI-DrugBank.d224.s0.e1" text="MAO inhibitors" type="group"/>
