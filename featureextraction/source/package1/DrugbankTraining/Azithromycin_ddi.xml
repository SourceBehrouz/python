    <sentence id="DDI-DrugBank.d53.s0" text="Co-administration of nelfinavir at steady-state with a single dose of azithromycin.">
        <entity charOffset="21-30" id="DDI-DrugBank.d53.s0.e0" text="nelfinavir" type="drug"/>
        <entity charOffset="70-81" id="DDI-DrugBank.d53.s0.e1" text="azithromycin" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d53.s0.e0" e2="DDI-DrugBank.d53.s0.e1" id="DDI-DrugBank.d53.s0.p0"/>
    </sentence>
    <sentence id="DDI-DrugBank.d53.s1" text="Co-administration of nelfinavir at steady-state with a single dose of azithromycin (2 x 600 mg tablets) results in increased azithromycin serum concentrations.">
        <entity charOffset="21-30" id="DDI-DrugBank.d53.s1.e0" text="nelfinavir" type="drug"/>
        <entity charOffset="70-81" id="DDI-DrugBank.d53.s1.e1" text="azithromycin" type="drug"/>
        <entity charOffset="125-136" id="DDI-DrugBank.d53.s1.e2" text="azithromycin" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d53.s1.e0" e2="DDI-DrugBank.d53.s1.e1" id="DDI-DrugBank.d53.s1.p0" type="effect"/>
        <pair ddi="false" e1="DDI-DrugBank.d53.s1.e0" e2="DDI-DrugBank.d53.s1.e2" id="DDI-DrugBank.d53.s1.p1"/>
