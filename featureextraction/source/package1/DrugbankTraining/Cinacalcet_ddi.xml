    </sentence>
    <sentence id="DDI-DrugBank.d512.s5" text="Ketoconazole: Sensipar is metabolized in part by CYP3A4.">
        <entity charOffset="0-11" id="DDI-DrugBank.d512.s5.e0" text="Ketoconazole" type="drug"/>
        <entity charOffset="14-21" id="DDI-DrugBank.d512.s5.e1" text="Sensipar" type="brand"/>
