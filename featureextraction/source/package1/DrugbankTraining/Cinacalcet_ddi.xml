    <sentence id="DDI-DrugBank.d512.s1" text="Effect of Sensipar on other drugs: Drugs metabolized by cytochrome P450 2D6 (CYP2D6): Sensipar  is a strong in vitro inhibitor of CYP2D6.">
        <entity charOffset="10-17" id="DDI-DrugBank.d512.s1.e0" text="Sensipar" type="brand"/>
        <entity charOffset="86-93" id="DDI-DrugBank.d512.s1.e1" text="Sensipar" type="brand"/>
