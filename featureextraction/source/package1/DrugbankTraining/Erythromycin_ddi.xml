    </sentence>
    <sentence id="DDI-DrugBank.d397.s3" text="There have been reports of increased anticoagulant effects when erythromycin and oral anticoagulants were used concomitantly.">
        <entity charOffset="64-75" id="DDI-DrugBank.d397.s3.e0" text="erythromycin" type="drug"/>
        <entity charOffset="86-99" id="DDI-DrugBank.d397.s3.e1" text="anticoagulants" type="group"/>
        <pair ddi="true" e1="DDI-DrugBank.d397.s3.e0" e2="DDI-DrugBank.d397.s3.e1" id="DDI-DrugBank.d397.s3.p0" type="effect"/>
    </sentence>
    <sentence id="DDI-DrugBank.d397.s4" text="Increased anticoagulation effects due to interactions of erythromycin with various oral anticoagulents may be more pronounced in the elderly.">
        <entity charOffset="57-68" id="DDI-DrugBank.d397.s4.e0" text="erythromycin" type="drug"/>
    </sentence>
    <sentence id="DDI-DrugBank.d397.s5" text="Concurrent use of erythromycin and ergotamine or dihydroergotamine has been associated in some patients with acute ergot toxicity characterized by severe peripheral vasospasm and dysesthesia.">
        <entity charOffset="18-29" id="DDI-DrugBank.d397.s5.e0" text="erythromycin" type="drug"/>
        <entity charOffset="35-44" id="DDI-DrugBank.d397.s5.e1" text="ergotamine" type="drug"/>
        <entity charOffset="49-65" id="DDI-DrugBank.d397.s5.e2" text="dihydroergotamine" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d397.s5.e0" e2="DDI-DrugBank.d397.s5.e1" id="DDI-DrugBank.d397.s5.p0" type="effect"/>
        <pair ddi="true" e1="DDI-DrugBank.d397.s5.e0" e2="DDI-DrugBank.d397.s5.e2" id="DDI-DrugBank.d397.s5.p1" type="effect"/>
        <pair ddi="false" e1="DDI-DrugBank.d397.s5.e1" e2="DDI-DrugBank.d397.s5.e2" id="DDI-DrugBank.d397.s5.p2"/>
    </sentence>
    <sentence id="DDI-DrugBank.d397.s6" text="Erythromycin has been reported to decrease the clearance of triazolam and midazolam and thus may increase the pharmacologic effect of these benzodiazepines.">
        <entity charOffset="0-11" id="DDI-DrugBank.d397.s6.e0" text="Erythromycin" type="drug"/>
