        <entity charOffset="74-82" id="DDI-DrugBank.d397.s6.e2" text="midazolam" type="drug"/>
        <entity charOffset="140-154" id="DDI-DrugBank.d397.s6.e3" text="benzodiazepines" type="group"/>
        <pair ddi="true" e1="DDI-DrugBank.d397.s6.e0" e2="DDI-DrugBank.d397.s6.e1" id="DDI-DrugBank.d397.s6.p0" type="mechanism"/>
        <pair ddi="true" e1="DDI-DrugBank.d397.s6.e0" e2="DDI-DrugBank.d397.s6.e2" id="DDI-DrugBank.d397.s6.p1" type="mechanism"/>
        <pair ddi="true" e1="DDI-DrugBank.d397.s6.e0" e2="DDI-DrugBank.d397.s6.e3" id="DDI-DrugBank.d397.s6.p2" type="effect"/>
