    <sentence id="DDI-DrugBank.d352.s0" text="Antidiabetic drug requirements (i.e., insulin) may be altered.">
        <entity charOffset="0-16" id="DDI-DrugBank.d352.s0.e0" text="Antidiabetic drug" type="group"/>
        <entity charOffset="38-44" id="DDI-DrugBank.d352.s0.e1" text="insulin" type="drug"/>
