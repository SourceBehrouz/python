        <entity charOffset="76-87" id="DDI-DrugBank.d352.s3.e2" text="guanethidine" type="drug"/>
        <entity charOffset="90-101" id="DDI-DrugBank.d352.s3.e3" text="a-methyldopa" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d352.s3.e0" e2="DDI-DrugBank.d352.s3.e1" id="DDI-DrugBank.d352.s3.p0" type="int"/>
