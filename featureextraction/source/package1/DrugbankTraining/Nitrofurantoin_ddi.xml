    <sentence id="DDI-DrugBank.d276.s3" text="The resulting increase in nitrofurantoin serum levels may increase toxicity, and the decreased urinary levels could lessen its efficacy as a urinary tract antibacterial.">
        <entity charOffset="26-39" id="DDI-DrugBank.d276.s3.e0" text="nitrofurantoin" type="drug"/>
        <entity charOffset="155-167" id="DDI-DrugBank.d276.s3.e1" text="antibacterial" type="group"/>
