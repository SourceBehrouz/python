    <sentence id="DDI-DrugBank.d76.s19" text="Diazepam: The co-administration of Fluvoxamine Tablets and diazepam is generally not advisable.">
        <entity charOffset="0-7" id="DDI-DrugBank.d76.s19.e0" text="Diazepam" type="drug"/>
        <entity charOffset="35-45" id="DDI-DrugBank.d76.s19.e1" text="Fluvoxamine" type="drug"/>
