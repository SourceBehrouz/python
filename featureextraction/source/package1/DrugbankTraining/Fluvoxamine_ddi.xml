        <entity charOffset="13-22" id="DDI-DrugBank.d76.s7.e1" text="astemizole" type="drug"/>
        <entity charOffset="28-36" id="DDI-DrugBank.d76.s7.e2" text="cisapride" type="drug"/>
        <entity charOffset="129-140" id="DDI-DrugBank.d76.s7.e3" text="ketoconazole" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d76.s7.e0" e2="DDI-DrugBank.d76.s7.e1" id="DDI-DrugBank.d76.s7.p0"/>
