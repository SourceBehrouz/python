    <sentence id="DDI-DrugBank.d434.s18" text="In nine otherwise healthy subjects with epilepsy ingesting carbamazepine, the steady-state trough (Cmin) carbamazepine concentration was 8 2 micrograms/mL.">
        <entity charOffset="59-71" id="DDI-DrugBank.d434.s18.e0" text="carbamazepine" type="drug"/>
        <entity charOffset="105-117" id="DDI-DrugBank.d434.s18.e1" text="carbamazepine" type="drug"/>
