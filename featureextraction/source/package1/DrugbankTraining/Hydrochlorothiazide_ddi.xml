    <sentence id="DDI-DrugBank.d162.s5" text="Single doses of either cholestyramine or colestipol resins bind the hydrochlorothiazide and reduce its absorption from the gastrointestinal tract by up to 85 and 43 percent, respectively.">
        <entity charOffset="23-36" id="DDI-DrugBank.d162.s5.e0" text="cholestyramine" type="drug"/>
        <entity charOffset="41-50" id="DDI-DrugBank.d162.s5.e1" text="colestipol" type="drug"/>
        <entity charOffset="52-57" id="DDI-DrugBank.d162.s5.e2" text="resins" type="group"/>
