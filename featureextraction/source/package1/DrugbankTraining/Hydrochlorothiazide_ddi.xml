        <pair ddi="true" e1="DDI-DrugBank.d162.s10.e0" e2="DDI-DrugBank.d162.s10.e2" id="DDI-DrugBank.d162.s10.p1" type="effect"/>
        <pair ddi="false" e1="DDI-DrugBank.d162.s10.e1" e2="DDI-DrugBank.d162.s10.e2" id="DDI-DrugBank.d162.s10.p2"/>
    </sentence>
    <sentence id="DDI-DrugBank.d162.s11" text="Refer to the package insert for lithium preparations before use of such preparations with Hydrochlorothiazide.">
        <entity charOffset="32-38" id="DDI-DrugBank.d162.s11.e0" text="lithium" type="drug"/>
        <entity charOffset="90-108" id="DDI-DrugBank.d162.s11.e1" text="Hydrochlorothiazide" type="drug"/>
