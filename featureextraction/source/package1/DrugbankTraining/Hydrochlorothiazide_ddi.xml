    <sentence id="DDI-DrugBank.d162.s9" text="Lithium: generally should not be given with diuretics.">
        <entity charOffset="0-6" id="DDI-DrugBank.d162.s9.e0" text="Lithium" type="drug"/>
        <entity charOffset="44-52" id="DDI-DrugBank.d162.s9.e1" text="diuretics" type="group"/>
        <pair ddi="true" e1="DDI-DrugBank.d162.s9.e0" e2="DDI-DrugBank.d162.s9.e1" id="DDI-DrugBank.d162.s9.p0" type="advise"/>
    <negationtags>Lithium: generally should &lt;scope&gt; &lt;cue&gt; not &lt;/cue&gt; be given with diuretics. &lt;/scope&gt;</negationtags></sentence>
    <sentence id="DDI-DrugBank.d162.s10" text="Diuretic agents reduce the renal clearance of lithium and add a high risk of lithium toxicity.">
        <entity charOffset="0-14" id="DDI-DrugBank.d162.s10.e0" text="Diuretic agents" type="group"/>
        <entity charOffset="46-52" id="DDI-DrugBank.d162.s10.e1" text="lithium" type="drug"/>
        <entity charOffset="77-83" id="DDI-DrugBank.d162.s10.e2" text="lithium" type="drug"/>
