        <entity charOffset="37-43" id="DDI-DrugBank.d162.s2.e1" text="insulin" type="drug"/>
        <entity charOffset="73-89" id="DDI-DrugBank.d162.s2.e2" text="antidiabetic drug" type="group"/>
