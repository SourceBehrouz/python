    <sentence id="DDI-DrugBank.d423.s1" text="Because dexfenfluramine is a serotonin releaser and reuptake inhibitor, dexfenfluramine should not be used concomitantly with a MAO inhibitor.">
        <entity charOffset="8-22" id="DDI-DrugBank.d423.s1.e0" text="dexfenfluramine" type="drug"/>
        <entity charOffset="72-86" id="DDI-DrugBank.d423.s1.e1" text="dexfenfluramine" type="drug"/>
        <entity charOffset="128-140" id="DDI-DrugBank.d423.s1.e2" text="MAO inhibitor" type="group"/>
        <pair ddi="false" e1="DDI-DrugBank.d423.s1.e0" e2="DDI-DrugBank.d423.s1.e1" id="DDI-DrugBank.d423.s1.p0"/>
