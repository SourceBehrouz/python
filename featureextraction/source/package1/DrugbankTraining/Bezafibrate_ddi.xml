        <pair ddi="true" e1="DDI-DrugBank.d291.s3.e0" e2="DDI-DrugBank.d291.s3.e3" id="DDI-DrugBank.d291.s3.p2" type="effect"/>
        <pair ddi="true" e1="DDI-DrugBank.d291.s3.e1" e2="DDI-DrugBank.d291.s3.e2" id="DDI-DrugBank.d291.s3.p3" type="effect"/>
