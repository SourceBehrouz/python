    <sentence id="DDI-DrugBank.d291.s1" text="For this reason, the dose of the anticoagulant should be reduced by 30 - 50% at the start of treatment with Bezalip or Bezalip retard and then titrated according to the blood clotting parameters">
        <entity charOffset="33-45" id="DDI-DrugBank.d291.s1.e0" text="anticoagulant" type="drug"/>
        <entity charOffset="108-114" id="DDI-DrugBank.d291.s1.e1" text="Bezalip" type="brand"/>
