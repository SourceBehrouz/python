        <pair ddi="true" e1="DDI-DrugBank.d249.s13.e0" e2="DDI-DrugBank.d249.s13.e2" id="DDI-DrugBank.d249.s13.p1" type="effect"/>
        <pair ddi="false" e1="DDI-DrugBank.d249.s13.e1" e2="DDI-DrugBank.d249.s13.e2" id="DDI-DrugBank.d249.s13.p2"/>
