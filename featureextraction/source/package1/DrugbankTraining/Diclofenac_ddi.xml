    <sentence id="DDI-DrugBank.d249.s10" text="Oral Hypoglycemics: Diclofenac does not alter glucose metabolism in normal subjects nor does it alter the effects of oral hypoglycemic agents.">
        <entity charOffset="5-17" id="DDI-DrugBank.d249.s10.e0" text="Hypoglycemics" type="group"/>
        <entity charOffset="20-29" id="DDI-DrugBank.d249.s10.e1" text="Diclofenac" type="drug"/>
