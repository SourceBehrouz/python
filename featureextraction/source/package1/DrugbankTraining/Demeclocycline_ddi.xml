    <sentence id="DDI-DrugBank.d409.s1" text="Since bacteriostatic drugs, such as the tetracycline class of antibiotics, may interfere with the bactericidal action of penicillins, it is not advisable to administer these drugs concomitantly.">
        <entity charOffset="40-57" id="DDI-DrugBank.d409.s1.e0" text="tetracycline class" type="group"/>
        <entity charOffset="62-72" id="DDI-DrugBank.d409.s1.e1" text="antibiotics" type="group"/>
