        <entity charOffset="94-102" id="DDI-DrugBank.d413.s6.e3" text="dicumarol" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d413.s6.e0" e2="DDI-DrugBank.d413.s6.e1" id="DDI-DrugBank.d413.s6.p0"/>
