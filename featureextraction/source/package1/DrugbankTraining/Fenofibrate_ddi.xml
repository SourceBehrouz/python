    </sentence>
    <sentence id="DDI-DrugBank.d283.s4" text="Resins: Since bile acid sequestrants may bind other drugs given concurrently, patients should take TRICOR at least 1 hour before or 4-6 hours after a bile acid binding resin to avoid impeding its absorption.">
        <entity charOffset="0-5" id="DDI-DrugBank.d283.s4.e0" text="Resins" type="group"/>
        <entity charOffset="14-35" id="DDI-DrugBank.d283.s4.e1" text="bile acid sequestrants" type="group"/>
        <entity charOffset="99-104" id="DDI-DrugBank.d283.s4.e2" text="TRICOR" type="brand"/>
        <entity charOffset="150-172" id="DDI-DrugBank.d283.s4.e3" text="bile acid binding resin" type="group"/>
        <pair ddi="false" e1="DDI-DrugBank.d283.s4.e0" e2="DDI-DrugBank.d283.s4.e1" id="DDI-DrugBank.d283.s4.p0"/>
        <pair ddi="false" e1="DDI-DrugBank.d283.s4.e0" e2="DDI-DrugBank.d283.s4.e2" id="DDI-DrugBank.d283.s4.p1"/>
