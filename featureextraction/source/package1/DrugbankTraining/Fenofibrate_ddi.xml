        <pair ddi="false" e1="DDI-DrugBank.d283.s13.e1" e2="DDI-DrugBank.d283.s13.e3" id="DDI-DrugBank.d283.s13.p4"/>
        <pair ddi="false" e1="DDI-DrugBank.d283.s13.e2" e2="DDI-DrugBank.d283.s13.e3" id="DDI-DrugBank.d283.s13.p5"/>
    </sentence>
    <sentence id="DDI-DrugBank.d283.s14" text="A single dose of pravastatin had no clinically important effect on the pharmacokinetics of fenofibric acid.">
        <entity charOffset="17-27" id="DDI-DrugBank.d283.s14.e0" text="pravastatin" type="drug"/>
        <entity charOffset="91-105" id="DDI-DrugBank.d283.s14.e1" text="fenofibric acid" type="drug_n"/>
