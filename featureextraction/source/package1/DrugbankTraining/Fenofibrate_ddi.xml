    <sentence id="DDI-DrugBank.d283.s0" text="Oral Anticoagulants CAUTION SHOULD BE EXERCISED WHEN COUMARIN ANTICOAGULANTS ARE GIVEN IN CONJUNCTION WITH TRICOR.">
        <entity charOffset="5-18" id="DDI-DrugBank.d283.s0.e0" text="Anticoagulants" type="group"/>
        <entity charOffset="53-75" id="DDI-DrugBank.d283.s0.e1" text="COUMARIN ANTICOAGULANTS" type="group"/>
        <entity charOffset="107-112" id="DDI-DrugBank.d283.s0.e2" text="TRICOR" type="brand"/>
        <pair ddi="false" e1="DDI-DrugBank.d283.s0.e0" e2="DDI-DrugBank.d283.s0.e1" id="DDI-DrugBank.d283.s0.p0"/>
        <pair ddi="false" e1="DDI-DrugBank.d283.s0.e0" e2="DDI-DrugBank.d283.s0.e2" id="DDI-DrugBank.d283.s0.p1"/>
        <pair ddi="true" e1="DDI-DrugBank.d283.s0.e1" e2="DDI-DrugBank.d283.s0.e2" id="DDI-DrugBank.d283.s0.p2" type="advise"/>
    </sentence>
    <sentence id="DDI-DrugBank.d283.s1" text="THE DOSAGE OF THE ANTICOAGULANTS SHOULD BE REDUCED TO MAINTAIN THE PROTHROMBIN TIME/INR AT THE DESIRED LEVEL TO PREVENT BLEEDING COMPLICATIONS.">
        <entity charOffset="18-31" id="DDI-DrugBank.d283.s1.e0" text="ANTICOAGULANTS" type="group"/>
    </sentence>
    <sentence id="DDI-DrugBank.d283.s2" text="FREQUENT PROTHROMBIN TIME/INR DETERMINATIONS ARE ADVISABLE UNTIL IT HAS BEEN DEFINITELY DETERMINED THAT THE PROTHROMBIN TIME/INR HAS STABILIZED."/>
    <sentence id="DDI-DrugBank.d283.s3" text="HMG-CoA reductase inhibitors: The combined use of TRICOR and HMG-CoA reductase inhibitors should be avoided unless the benefit of further alterations in lipid levels is likely to outweigh the increased risk of this drug combination.">
        <entity charOffset="0-27" id="DDI-DrugBank.d283.s3.e0" text="HMG-CoA reductase inhibitors" type="group"/>
        <entity charOffset="50-55" id="DDI-DrugBank.d283.s3.e1" text="TRICOR" type="brand"/>
        <entity charOffset="61-88" id="DDI-DrugBank.d283.s3.e2" text="HMG-CoA reductase inhibitors" type="group"/>
        <pair ddi="false" e1="DDI-DrugBank.d283.s3.e0" e2="DDI-DrugBank.d283.s3.e1" id="DDI-DrugBank.d283.s3.p0"/>
        <pair ddi="false" e1="DDI-DrugBank.d283.s3.e0" e2="DDI-DrugBank.d283.s3.e2" id="DDI-DrugBank.d283.s3.p1"/>
