    <sentence id="DDI-DrugBank.d11.s1" text="CYP2D6 inhibitors - Atomoxetine is primarily metabolized by the CYP2D6 pathway to 4-hydroxyatomoxetine.">
        <entity charOffset="20-30" id="DDI-DrugBank.d11.s1.e0" text="Atomoxetine" type="drug"/>
        <entity charOffset="82-101" id="DDI-DrugBank.d11.s1.e1" text="4-hydroxyatomoxetine" type="drug_n"/>
