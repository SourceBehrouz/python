    <sentence id="DDI-DrugBank.d478.s2" text="The administration of naratriptan with other 5-HT1 agonists has not been evaluated in migraine patients.">
        <entity charOffset="22-32" id="DDI-DrugBank.d478.s2.e0" text="naratriptan" type="drug"/>
