        <entity charOffset="122-133" id="DDI-DrugBank.d66.s2.e3" text="indomethacin" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d66.s2.e0" e2="DDI-DrugBank.d66.s2.e1" id="DDI-DrugBank.d66.s2.p0"/>
