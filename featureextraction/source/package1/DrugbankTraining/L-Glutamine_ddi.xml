    </sentence>
    <sentence id="DDI-DrugBank.d66.s1" text="L-glutamine has orphan drug status for this indication.">
        <entity charOffset="0-10" id="DDI-DrugBank.d66.s1.e0" text="L-glutamine" type="drug"/>
    </sentence>
    <sentence id="DDI-DrugBank.d66.s2" text="Indomethacin - Concomitant use of L-glutamine and indomethacin may ameliorate increased intestinal permeability caused by indomethacin.">
        <entity charOffset="0-11" id="DDI-DrugBank.d66.s2.e0" text="Indomethacin" type="drug"/>
        <entity charOffset="34-44" id="DDI-DrugBank.d66.s2.e1" text="L-glutamine" type="drug"/>
