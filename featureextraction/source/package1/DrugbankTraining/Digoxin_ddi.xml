        <pair ddi="false" e1="DDI-DrugBank.d450.s7.e1" e2="DDI-DrugBank.d450.s7.e2" id="DDI-DrugBank.d450.s7.p2"/>
    </sentence>
    <sentence id="DDI-DrugBank.d450.s8" text="There have been inconsistent reports regarding the effects of other drugs (e.g., quinine, penicillamine) on serum digoxin concentration.">
        <entity charOffset="81-87" id="DDI-DrugBank.d450.s8.e0" text="quinine" type="drug"/>
        <entity charOffset="90-102" id="DDI-DrugBank.d450.s8.e1" text="penicillamine" type="drug"/>
        <entity charOffset="114-120" id="DDI-DrugBank.d450.s8.e2" text="digoxin" type="drug"/>
