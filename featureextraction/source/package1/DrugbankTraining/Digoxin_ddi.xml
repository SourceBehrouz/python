        <pair ddi="false" e1="DDI-DrugBank.d450.s8.e1" e2="DDI-DrugBank.d450.s8.e2" id="DDI-DrugBank.d450.s8.p2"/>
    </sentence>
    <sentence id="DDI-DrugBank.d450.s9" text="Thyroid administration to a digitalized, hypothyroid patient may increase the dose requirement of digoxin.">
        <entity charOffset="0-6" id="DDI-DrugBank.d450.s9.e0" text="Thyroid" type="group"/>
        <entity charOffset="98-104" id="DDI-DrugBank.d450.s9.e1" text="digoxin" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d450.s9.e0" e2="DDI-DrugBank.d450.s9.e1" id="DDI-DrugBank.d450.s9.p0" type="advise"/>
    </sentence>
    <sentence id="DDI-DrugBank.d450.s10" text="Concomitant use of digoxin and sympathomimetics increases the risk of cardiac arrhythmias.">
        <entity charOffset="19-25" id="DDI-DrugBank.d450.s10.e0" text="digoxin" type="drug"/>
        <entity charOffset="31-46" id="DDI-DrugBank.d450.s10.e1" text="sympathomimetics" type="group"/>
        <pair ddi="true" e1="DDI-DrugBank.d450.s10.e0" e2="DDI-DrugBank.d450.s10.e1" id="DDI-DrugBank.d450.s10.p0" type="effect"/>
    </sentence>
    <sentence id="DDI-DrugBank.d450.s11" text="Succinylcholine may cause a sudden extrusion of potassium from muscle cells, and may thereby cause arrhythmias in digitalized patients.">
        <entity charOffset="0-14" id="DDI-DrugBank.d450.s11.e0" text="Succinylcholine" type="drug"/>
    </sentence>
    <sentence id="DDI-DrugBank.d450.s12" text="Although beta-adrenergic blockers or calcium channel blockers and digoxin may be useful in combination to control atrial fibrillation, their additive effects on AV node conduction can result in advanced or complete heart block.">
        <entity charOffset="9-32" id="DDI-DrugBank.d450.s12.e0" text="beta-adrenergic blockers" type="group"/>
        <entity charOffset="37-60" id="DDI-DrugBank.d450.s12.e1" text="calcium channel blockers" type="group"/>
        <entity charOffset="66-72" id="DDI-DrugBank.d450.s12.e2" text="digoxin" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d450.s12.e0" e2="DDI-DrugBank.d450.s12.e1" id="DDI-DrugBank.d450.s12.p0"/>
        <pair ddi="true" e1="DDI-DrugBank.d450.s12.e0" e2="DDI-DrugBank.d450.s12.e2" id="DDI-DrugBank.d450.s12.p1" type="effect"/>
        <pair ddi="true" e1="DDI-DrugBank.d450.s12.e1" e2="DDI-DrugBank.d450.s12.e2" id="DDI-DrugBank.d450.s12.p2" type="effect"/>
    </sentence>
    <sentence id="DDI-DrugBank.d450.s13" text="Due to the considerable variability of these interactions, the dosage of digoxin should be individualized when patients receive these medications concurrently.">
        <entity charOffset="73-79" id="DDI-DrugBank.d450.s13.e0" text="digoxin" type="drug"/>
    </sentence>
    <sentence id="DDI-DrugBank.d450.s14" text="Furthermore, caution should be exercised when combining digoxin with any drug that may cause a significant deterioration in renal function, since a decline in glomerular filtration or tubular secretion may impair the excretion of digoxin.">
        <entity charOffset="56-62" id="DDI-DrugBank.d450.s14.e0" text="digoxin" type="drug"/>
