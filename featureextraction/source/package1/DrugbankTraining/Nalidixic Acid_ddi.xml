<?xml version="1.0" encoding="UTF-8" standalone="no"?><document id="DDI-DrugBank.d427">
    <sentence id="DDI-DrugBank.d427.s0" text="Elevated plasma levels of theophylline have been reported with concomitant quinolone use.">
        <entity charOffset="26-37" id="DDI-DrugBank.d427.s0.e0" text="theophylline" type="drug"/>
        <entity charOffset="75-83" id="DDI-DrugBank.d427.s0.e1" text="quinolone" type="group"/>
        <pair ddi="true" e1="DDI-DrugBank.d427.s0.e0" e2="DDI-DrugBank.d427.s0.e1" id="DDI-DrugBank.d427.s0.p0" type="mechanism"/>
    </sentence>
    <sentence id="DDI-DrugBank.d427.s1" text="There have been reports of theophylline-related side effects in patients on concomitant therapy with quinolones and theophylline.">
        <entity charOffset="27-38" id="DDI-DrugBank.d427.s1.e0" text="theophylline" type="drug"/>
        <entity charOffset="101-110" id="DDI-DrugBank.d427.s1.e1" text="quinolones" type="group"/>
        <entity charOffset="116-127" id="DDI-DrugBank.d427.s1.e2" text="theophylline" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d427.s1.e0" e2="DDI-DrugBank.d427.s1.e1" id="DDI-DrugBank.d427.s1.p0"/>
        <pair ddi="false" e1="DDI-DrugBank.d427.s1.e0" e2="DDI-DrugBank.d427.s1.e2" id="DDI-DrugBank.d427.s1.p1"/>
        <pair ddi="true" e1="DDI-DrugBank.d427.s1.e1" e2="DDI-DrugBank.d427.s1.e2" id="DDI-DrugBank.d427.s1.p2" type="effect"/>
    </sentence>
    <sentence id="DDI-DrugBank.d427.s2" text="Therefore, monitoring of theophylline plasma levels should be considered and dosage of theophylline adjusted, as required.">
        <entity charOffset="25-36" id="DDI-DrugBank.d427.s2.e0" text="theophylline" type="drug"/>
        <entity charOffset="87-98" id="DDI-DrugBank.d427.s2.e1" text="theophylline" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d427.s2.e0" e2="DDI-DrugBank.d427.s2.e1" id="DDI-DrugBank.d427.s2.p0"/>
    </sentence>
    <sentence id="DDI-DrugBank.d427.s3" text="Quinolones have been shown to interfere with the metabolism of caffeine.">
        <entity charOffset="0-9" id="DDI-DrugBank.d427.s3.e0" text="Quinolones" type="group"/>
        <entity charOffset="63-70" id="DDI-DrugBank.d427.s3.e1" text="caffeine" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d427.s3.e0" e2="DDI-DrugBank.d427.s3.e1" id="DDI-DrugBank.d427.s3.p0" type="mechanism"/>
    </sentence>
    <sentence id="DDI-DrugBank.d427.s4" text="This may lead to reduced clearance of caffeine and the prolongation of its plasma half-life.">
        <entity charOffset="38-45" id="DDI-DrugBank.d427.s4.e0" text="caffeine" type="drug"/>
    </sentence>
    <sentence id="DDI-DrugBank.d427.s5" text="Quinolones, including nalidixic acid, may enhance the effects of the oral anticoagulant warfarin or its derivatives.">
        <entity charOffset="0-9" id="DDI-DrugBank.d427.s5.e0" text="Quinolones" type="group"/>
        <entity charOffset="22-35" id="DDI-DrugBank.d427.s5.e1" text="nalidixic acid" type="drug"/>
        <entity charOffset="74-86" id="DDI-DrugBank.d427.s5.e2" text="anticoagulant" type="group"/>
        <entity charOffset="88-95" id="DDI-DrugBank.d427.s5.e3" text="warfarin" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d427.s5.e0" e2="DDI-DrugBank.d427.s5.e1" id="DDI-DrugBank.d427.s5.p0"/>
        <pair ddi="false" e1="DDI-DrugBank.d427.s5.e0" e2="DDI-DrugBank.d427.s5.e2" id="DDI-DrugBank.d427.s5.p1"/>
        <pair ddi="true" e1="DDI-DrugBank.d427.s5.e0" e2="DDI-DrugBank.d427.s5.e3" id="DDI-DrugBank.d427.s5.p2" type="effect"/>
        <pair ddi="false" e1="DDI-DrugBank.d427.s5.e1" e2="DDI-DrugBank.d427.s5.e2" id="DDI-DrugBank.d427.s5.p3"/>
        <pair ddi="true" e1="DDI-DrugBank.d427.s5.e1" e2="DDI-DrugBank.d427.s5.e3" id="DDI-DrugBank.d427.s5.p4" type="effect"/>
        <pair ddi="false" e1="DDI-DrugBank.d427.s5.e2" e2="DDI-DrugBank.d427.s5.e3" id="DDI-DrugBank.d427.s5.p5"/>
    </sentence>
    <sentence id="DDI-DrugBank.d427.s6" text="When these products are administered concomitantly, prothrombin time or other suitable coagulation test should be closely monitored."/>
    <sentence id="DDI-DrugBank.d427.s7" text="Nitrofurantoin interferes with the therapeutic action of nalidixic acid.">
        <entity charOffset="0-13" id="DDI-DrugBank.d427.s7.e0" text="Nitrofurantoin" type="drug"/>
        <entity charOffset="57-70" id="DDI-DrugBank.d427.s7.e1" text="nalidixic acid" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d427.s7.e0" e2="DDI-DrugBank.d427.s7.e1" id="DDI-DrugBank.d427.s7.p0" type="effect"/>
    </sentence>
    <sentence id="DDI-DrugBank.d427.s8" text="Antacids containing magnesium, aluminum, or calcium;">
        <entity charOffset="0-7" id="DDI-DrugBank.d427.s8.e0" text="Antacids" type="group"/>
        <entity charOffset="20-28" id="DDI-DrugBank.d427.s8.e1" text="magnesium" type="drug"/>
        <entity charOffset="31-38" id="DDI-DrugBank.d427.s8.e2" text="aluminum" type="drug"/>
        <entity charOffset="44-50" id="DDI-DrugBank.d427.s8.e3" text="calcium" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d427.s8.e0" e2="DDI-DrugBank.d427.s8.e1" id="DDI-DrugBank.d427.s8.p0"/>
        <pair ddi="false" e1="DDI-DrugBank.d427.s8.e0" e2="DDI-DrugBank.d427.s8.e2" id="DDI-DrugBank.d427.s8.p1"/>
        <pair ddi="false" e1="DDI-DrugBank.d427.s8.e0" e2="DDI-DrugBank.d427.s8.e3" id="DDI-DrugBank.d427.s8.p2"/>
        <pair ddi="false" e1="DDI-DrugBank.d427.s8.e1" e2="DDI-DrugBank.d427.s8.e2" id="DDI-DrugBank.d427.s8.p3"/>
        <pair ddi="false" e1="DDI-DrugBank.d427.s8.e1" e2="DDI-DrugBank.d427.s8.e3" id="DDI-DrugBank.d427.s8.p4"/>
        <pair ddi="false" e1="DDI-DrugBank.d427.s8.e2" e2="DDI-DrugBank.d427.s8.e3" id="DDI-DrugBank.d427.s8.p5"/>
    </sentence>
    <sentence id="DDI-DrugBank.d427.s9" text="sucralfate or divalent or trivalent cations such as iron;">
        <entity charOffset="0-9" id="DDI-DrugBank.d427.s9.e0" text="sucralfate" type="drug"/>
        <entity charOffset="52-55" id="DDI-DrugBank.d427.s9.e1" text="iron" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d427.s9.e0" e2="DDI-DrugBank.d427.s9.e1" id="DDI-DrugBank.d427.s9.p0"/>
    </sentence>
    <sentence id="DDI-DrugBank.d427.s10" text="multivitamins containing zinc;">
        <entity charOffset="0-12" id="DDI-DrugBank.d427.s10.e0" text="multivitamins" type="group"/>
        <entity charOffset="25-28" id="DDI-DrugBank.d427.s10.e1" text="zinc" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d427.s10.e0" e2="DDI-DrugBank.d427.s10.e1" id="DDI-DrugBank.d427.s10.p0"/>
    </sentence>
    <sentence id="DDI-DrugBank.d427.s11" text="and Videx , (Didanosine), chewable/buffered tablets or the pediatric powder for oral solution may substantially interfere with the absorption of quinolones, resulting in systemic levels considerably lower than desired.">
        <entity charOffset="4-8" id="DDI-DrugBank.d427.s11.e0" text="Videx" type="brand"/>
        <entity charOffset="13-22" id="DDI-DrugBank.d427.s11.e1" text="Didanosine" type="drug"/>
        <entity charOffset="145-154" id="DDI-DrugBank.d427.s11.e2" text="quinolones" type="group"/>
        <pair ddi="false" e1="DDI-DrugBank.d427.s11.e0" e2="DDI-DrugBank.d427.s11.e1" id="DDI-DrugBank.d427.s11.p0"/>
        <pair ddi="true" e1="DDI-DrugBank.d427.s11.e0" e2="DDI-DrugBank.d427.s11.e2" id="DDI-DrugBank.d427.s11.p1" type="mechanism"/>
        <pair ddi="true" e1="DDI-DrugBank.d427.s11.e1" e2="DDI-DrugBank.d427.s11.e2" id="DDI-DrugBank.d427.s11.p2" type="mechanism"/>
    </sentence>
    <sentence id="DDI-DrugBank.d427.s12" text="These agents should not be taken within the two hour period before or within the two-hour period after nalidixic acid administration.">
        <entity charOffset="103-116" id="DDI-DrugBank.d427.s12.e0" text="nalidixic acid" type="drug"/>
    <negationtags>These agents should &lt;scope&gt; &lt;cue&gt; not &lt;/cue&gt; be taken within the two hour period before or within the two-hour period after nalidixic acid administration. &lt;/scope&gt;</negationtags></sentence>
    <sentence id="DDI-DrugBank.d427.s13" text="Elevated serum levels of cyclosporine have been reported with the concomitant use of some quinolones and cyclosporine.">
        <entity charOffset="25-36" id="DDI-DrugBank.d427.s13.e0" text="cyclosporine" type="drug"/>
        <entity charOffset="90-99" id="DDI-DrugBank.d427.s13.e1" text="quinolones" type="group"/>
        <entity charOffset="105-116" id="DDI-DrugBank.d427.s13.e2" text="cyclosporine" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d427.s13.e0" e2="DDI-DrugBank.d427.s13.e1" id="DDI-DrugBank.d427.s13.p0"/>
        <pair ddi="false" e1="DDI-DrugBank.d427.s13.e0" e2="DDI-DrugBank.d427.s13.e2" id="DDI-DrugBank.d427.s13.p1"/>
        <pair ddi="true" e1="DDI-DrugBank.d427.s13.e1" e2="DDI-DrugBank.d427.s13.e2" id="DDI-DrugBank.d427.s13.p2" type="mechanism"/>
    </sentence>
    <sentence id="DDI-DrugBank.d427.s14" text="Therefore, cyclosporine serum levels should be monitored and appropriate cyclosporine dosage adjustments made when these drugs are used concomitantly.">
        <entity charOffset="11-22" id="DDI-DrugBank.d427.s14.e0" text="cyclosporine" type="drug"/>
        <entity charOffset="73-84" id="DDI-DrugBank.d427.s14.e1" text="cyclosporine" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d427.s14.e0" e2="DDI-DrugBank.d427.s14.e1" id="DDI-DrugBank.d427.s14.p0"/>
    </sentence>
