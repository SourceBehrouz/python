    </sentence>
    <sentence id="DDI-DrugBank.d526.s4" text="Digitalis glycosides enhanced possibility of arrhythmias or digitalis toxicity associated with hypokalemia.">
        <entity charOffset="0-19" id="DDI-DrugBank.d526.s4.e0" text="Digitalis glycosides" type="group"/>
