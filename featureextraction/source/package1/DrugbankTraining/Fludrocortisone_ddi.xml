    <sentence id="DDI-DrugBank.d526.s5" text="Monitor serum potassium levels;"/>
    <sentence id="DDI-DrugBank.d526.s6" text="use potassium supplements if necessary.">
        <entity charOffset="4-12" id="DDI-DrugBank.d526.s6.e0" text="potassium" type="drug"/>
    </sentence>
    <sentence id="DDI-DrugBank.d526.s7" text="Oral anticoagulants decreased prothrombin time response.">
        <entity charOffset="5-18" id="DDI-DrugBank.d526.s7.e0" text="anticoagulants" type="group"/>
    </sentence>
    <sentence id="DDI-DrugBank.d526.s8" text="Monitor prothrombin levels and adjust anticoagulant dosage accordingly.">
        <entity charOffset="38-50" id="DDI-DrugBank.d526.s8.e0" text="anticoagulant" type="group"/>
    </sentence>
    <sentence id="DDI-DrugBank.d526.s9" text="Antidiabetic drugs (oral agents and insulin) diminished antidiabetic effect.">
        <entity charOffset="0-17" id="DDI-DrugBank.d526.s9.e0" text="Antidiabetic drugs" type="group"/>
        <entity charOffset="36-42" id="DDI-DrugBank.d526.s9.e1" text="insulin" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d526.s9.e0" e2="DDI-DrugBank.d526.s9.e1" id="DDI-DrugBank.d526.s9.p0"/>
    </sentence>
    <sentence id="DDI-DrugBank.d526.s10" text="Monitor for symptoms of hyperglycemia;"/>
    <sentence id="DDI-DrugBank.d526.s11" text="adjust dosage of antidiabetic drug upward if necessary.">
        <entity charOffset="17-33" id="DDI-DrugBank.d526.s11.e0" text="antidiabetic drug" type="group"/>
    </sentence>
    <sentence id="DDI-DrugBank.d526.s12" text="Aspirin increased ulcerogenic effect;">
        <entity charOffset="0-6" id="DDI-DrugBank.d526.s12.e0" text="Aspirin" type="brand"/>
    </sentence>
    <sentence id="DDI-DrugBank.d526.s13" text="decreased pharmacologic effect of aspirin.">
        <entity charOffset="34-40" id="DDI-DrugBank.d526.s13.e0" text="aspirin" type="brand"/>
    </sentence>
    <sentence id="DDI-DrugBank.d526.s14" text="Rarely salicylate toxicity may occur in patients who discontinue steroids after concurrent high-dose aspirin therapy.">
