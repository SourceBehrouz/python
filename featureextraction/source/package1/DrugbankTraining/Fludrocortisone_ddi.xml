    </sentence>
    <sentence id="DDI-DrugBank.d526.s15" text="Monitor salicylate levels or the therapeutic effect for which aspirin is given;">
        <entity charOffset="8-17" id="DDI-DrugBank.d526.s15.e0" text="salicylate" type="group"/>
        <entity charOffset="62-68" id="DDI-DrugBank.d526.s15.e1" text="aspirin" type="brand"/>
