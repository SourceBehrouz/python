    <sentence id="DDI-DrugBank.d178.s9" text="A potential interaction between oral miconazole and oral hypoglycemic agents leading to severe hypoglycemia has been reported.">
        <entity charOffset="37-46" id="DDI-DrugBank.d178.s9.e0" text="miconazole" type="drug"/>
        <entity charOffset="57-75" id="DDI-DrugBank.d178.s9.e1" text="hypoglycemic agents" type="group"/>
