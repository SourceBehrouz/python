    </sentence>
    <sentence id="DDI-DrugBank.d141.s5" text="However, in the second study, administration of 12 g cholestyramine 1 hour before the evening meal and 0.3 mg cerivastatin sodium approximately 4 hours after the same evening meal resulted in a decrease in the cerivastatin AUC of less than 8%, and a decrease in Cmax of about 30% when compared to dosing cerivastatin sodium alone.">
        <entity charOffset="53-66" id="DDI-DrugBank.d141.s5.e0" text="cholestyramine" type="drug"/>
        <entity charOffset="110-128" id="DDI-DrugBank.d141.s5.e1" text="cerivastatin sodium" type="drug"/>
        <entity charOffset="210-221" id="DDI-DrugBank.d141.s5.e2" text="cerivastatin" type="drug"/>
        <entity charOffset="304-322" id="DDI-DrugBank.d141.s5.e3" text="cerivastatin sodium" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d141.s5.e0" e2="DDI-DrugBank.d141.s5.e1" id="DDI-DrugBank.d141.s5.p0" type="mechanism"/>
        <pair ddi="false" e1="DDI-DrugBank.d141.s5.e0" e2="DDI-DrugBank.d141.s5.e2" id="DDI-DrugBank.d141.s5.p1"/>
