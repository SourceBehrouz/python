    <sentence id="DDI-DrugBank.d141.s4" text="In the first study, concomitant administration of 0.2 mg cerivastatin sodium and 12 g cholestyramine resulted in decreases of more than 22% for AUC and 40% for Cmax when compared to dosing cerivastatin sodium alone.">
        <entity charOffset="57-75" id="DDI-DrugBank.d141.s4.e0" text="cerivastatin sodium" type="drug"/>
        <entity charOffset="86-99" id="DDI-DrugBank.d141.s4.e1" text="cholestyramine" type="drug"/>
        <entity charOffset="189-207" id="DDI-DrugBank.d141.s4.e2" text="cerivastatin sodium" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d141.s4.e0" e2="DDI-DrugBank.d141.s4.e1" id="DDI-DrugBank.d141.s4.p0" type="mechanism"/>
        <pair ddi="false" e1="DDI-DrugBank.d141.s4.e0" e2="DDI-DrugBank.d141.s4.e2" id="DDI-DrugBank.d141.s4.p1"/>
