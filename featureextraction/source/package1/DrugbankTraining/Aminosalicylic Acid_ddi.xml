    <sentence id="DDI-DrugBank.d22.s2" text="Aminosalicylic acid may also decrease the absorption of vitamin B12, which can lead to a deficiency.">
        <entity charOffset="0-18" id="DDI-DrugBank.d22.s2.e0" text="Aminosalicylic acid" type="drug"/>
        <entity charOffset="56-66" id="DDI-DrugBank.d22.s2.e1" text="vitamin B12" type="drug"/>
