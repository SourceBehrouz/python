    <sentence id="DDI-DrugBank.d498.s0" text="Certain antibiotics, especially neomycin, streptomycin and kanamycin, have a mild but definite nondepolarizing blocking action which may accentuate neuromuscular block.">
        <entity charOffset="8-18" id="DDI-DrugBank.d498.s0.e0" text="antibiotics" type="group"/>
        <entity charOffset="32-39" id="DDI-DrugBank.d498.s0.e1" text="neomycin" type="drug"/>
        <entity charOffset="42-53" id="DDI-DrugBank.d498.s0.e2" text="streptomycin" type="drug"/>
        <entity charOffset="59-67" id="DDI-DrugBank.d498.s0.e3" text="kanamycin" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d498.s0.e0" e2="DDI-DrugBank.d498.s0.e1" id="DDI-DrugBank.d498.s0.p0"/>
        <pair ddi="false" e1="DDI-DrugBank.d498.s0.e0" e2="DDI-DrugBank.d498.s0.e2" id="DDI-DrugBank.d498.s0.p1"/>
        <pair ddi="false" e1="DDI-DrugBank.d498.s0.e0" e2="DDI-DrugBank.d498.s0.e3" id="DDI-DrugBank.d498.s0.p2"/>
        <pair ddi="false" e1="DDI-DrugBank.d498.s0.e1" e2="DDI-DrugBank.d498.s0.e2" id="DDI-DrugBank.d498.s0.p3"/>
        <pair ddi="false" e1="DDI-DrugBank.d498.s0.e1" e2="DDI-DrugBank.d498.s0.e3" id="DDI-DrugBank.d498.s0.p4"/>
        <pair ddi="false" e1="DDI-DrugBank.d498.s0.e2" e2="DDI-DrugBank.d498.s0.e3" id="DDI-DrugBank.d498.s0.p5"/>
    </sentence>
    <sentence id="DDI-DrugBank.d498.s1" text="These antibiotics should be used in the myasthenic patient only where definitely indicated, and then careful adjustment should be made of adjunctive anticholinesterase dosage.">
        <entity charOffset="6-16" id="DDI-DrugBank.d498.s1.e0" text="antibiotics" type="group"/>
        <entity charOffset="149-166" id="DDI-DrugBank.d498.s1.e1" text="anticholinesterase" type="group"/>
        <pair ddi="true" e1="DDI-DrugBank.d498.s1.e0" e2="DDI-DrugBank.d498.s1.e1" id="DDI-DrugBank.d498.s1.p0" type="advise"/>
    </sentence>
    <sentence id="DDI-DrugBank.d498.s2" text="Local and some general anesthetics, antiarrhythmic agents and other drugs that interfere with neuromuscular transmission should be used cautiously, if at all, in patients with myasthenia gravis;">
        <entity charOffset="23-33" id="DDI-DrugBank.d498.s2.e0" text="anesthetics" type="group"/>
        <entity charOffset="36-56" id="DDI-DrugBank.d498.s2.e1" text="antiarrhythmic agents" type="group"/>
        <pair ddi="false" e1="DDI-DrugBank.d498.s2.e0" e2="DDI-DrugBank.d498.s2.e1" id="DDI-DrugBank.d498.s2.p0"/>
    </sentence>
    <sentence id="DDI-DrugBank.d498.s3" text="the dose of Prostigmin may have to be increased accordingly.">
        <entity charOffset="12-21" id="DDI-DrugBank.d498.s3.e0" text="Prostigmin" type="brand"/>
    </sentence>
