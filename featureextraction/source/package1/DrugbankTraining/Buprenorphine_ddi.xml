    <sentence id="DDI-DrugBank.d380.s3" text="Based on anecdotal reports, there may be an interaction between buprenorphine and benzodiazepines.">
        <entity charOffset="64-76" id="DDI-DrugBank.d380.s3.e0" text="buprenorphine" type="drug"/>
        <entity charOffset="82-96" id="DDI-DrugBank.d380.s3.e1" text="benzodiazepines" type="group"/>
        <pair ddi="true" e1="DDI-DrugBank.d380.s3.e0" e2="DDI-DrugBank.d380.s3.e1" id="DDI-DrugBank.d380.s3.p0" type="int"/>
    </sentence>
    <sentence id="DDI-DrugBank.d380.s4" text="There have been a number of reports in the post-marketing experience of coma and death associated with the concomitant intravenous misuse of buprenorphine and benzodiazepines by addicts.">
        <entity charOffset="141-153" id="DDI-DrugBank.d380.s4.e0" text="buprenorphine" type="drug"/>
        <entity charOffset="159-173" id="DDI-DrugBank.d380.s4.e1" text="benzodiazepines" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d380.s4.e0" e2="DDI-DrugBank.d380.s4.e1" id="DDI-DrugBank.d380.s4.p0" type="effect"/>
    </sentence>
    <sentence id="DDI-DrugBank.d380.s5" text="In many of these cases, buprenorphine was misused by self-injection of crushed SUBUTEX tablets.">
        <entity charOffset="24-36" id="DDI-DrugBank.d380.s5.e0" text="buprenorphine" type="drug"/>
        <entity charOffset="79-85" id="DDI-DrugBank.d380.s5.e1" text="SUBUTEX" type="brand"/>
        <pair ddi="false" e1="DDI-DrugBank.d380.s5.e0" e2="DDI-DrugBank.d380.s5.e1" id="DDI-DrugBank.d380.s5.p0"/>
    </sentence>
    <sentence id="DDI-DrugBank.d380.s6" text="SUBUTEX and SUBOXONE should be prescribed with caution to patients on benzodiazepines or other drugs that act on the central nervous system, regardless of whether these drugs are taken on the advice of a physician or are taken as drugs of abuse.">
        <entity charOffset="0-6" id="DDI-DrugBank.d380.s6.e0" text="SUBUTEX" type="brand"/>
        <entity charOffset="12-19" id="DDI-DrugBank.d380.s6.e1" text="SUBOXONE" type="brand"/>
        <entity charOffset="70-84" id="DDI-DrugBank.d380.s6.e2" text="benzodiazepines" type="group"/>
        <pair ddi="false" e1="DDI-DrugBank.d380.s6.e0" e2="DDI-DrugBank.d380.s6.e1" id="DDI-DrugBank.d380.s6.p0"/>
        <pair ddi="true" e1="DDI-DrugBank.d380.s6.e0" e2="DDI-DrugBank.d380.s6.e2" id="DDI-DrugBank.d380.s6.p1" type="advise"/>
        <pair ddi="true" e1="DDI-DrugBank.d380.s6.e1" e2="DDI-DrugBank.d380.s6.e2" id="DDI-DrugBank.d380.s6.p2" type="advise"/>
    </sentence>
    <sentence id="DDI-DrugBank.d380.s7" text="Patients should be warned of the potential danger of the intravenous self-administration of benzodiazepines while under treatment with SUBOXONE or SUBUTEX.">
        <entity charOffset="92-106" id="DDI-DrugBank.d380.s7.e0" text="benzodiazepines" type="group"/>
