    <sentence id="DDI-DrugBank.d380.s0" text="Buprenorphine is metabolized to norbuprenorphine by cytochrome CYP 3A4.">
        <entity charOffset="0-12" id="DDI-DrugBank.d380.s0.e0" text="Buprenorphine" type="drug"/>
        <entity charOffset="32-47" id="DDI-DrugBank.d380.s0.e1" text="norbuprenorphine" type="drug_n"/>
        <pair ddi="false" e1="DDI-DrugBank.d380.s0.e0" e2="DDI-DrugBank.d380.s0.e1" id="DDI-DrugBank.d380.s0.p0"/>
    </sentence>
    <sentence id="DDI-DrugBank.d380.s1" text="Because CYP 3A4 inhibitors may increase plasma concentrations of buprenorphine, patients already on CYP 3A4 inhibitors such as azole antifungals (e.g.">
