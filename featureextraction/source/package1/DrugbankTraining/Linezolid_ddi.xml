    </sentence>
    <sentence id="DDI-DrugBank.d441.s5" text="Serotonergic Agents: Co-administration of linezolid and serotonergic agents was not associated with serotonin syndrome in Phase 1, 2 or 3 studies.">
        <entity charOffset="0-18" id="DDI-DrugBank.d441.s5.e0" text="Serotonergic Agents" type="group"/>
        <entity charOffset="42-50" id="DDI-DrugBank.d441.s5.e1" text="linezolid" type="drug"/>
