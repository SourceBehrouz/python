    <sentence id="DDI-DrugBank.d514.s0" text="Opioids are strong central nervous system depressants, but regular users develop physiological tolerance allowing gradually increased dosages.">
        <entity charOffset="0-6" id="DDI-DrugBank.d514.s0.e0" text="Opioids" type="group"/>
        <entity charOffset="19-52" id="DDI-DrugBank.d514.s0.e1" text="central nervous system depressants" type="group"/>
        <pair ddi="false" e1="DDI-DrugBank.d514.s0.e0" e2="DDI-DrugBank.d514.s0.e1" id="DDI-DrugBank.d514.s0.p0"/>
    </sentence>
    <sentence id="DDI-DrugBank.d514.s1" text="In combination with other central nervous system depressants, heroin may still kill even experienced users, particularly if their tolerance to the drug has reduced or the strength of their usual dose has increased.">
        <entity charOffset="26-59" id="DDI-DrugBank.d514.s1.e0" text="central nervous system depressants" type="group"/>
        <entity charOffset="62-67" id="DDI-DrugBank.d514.s1.e1" text="heroin" type="drug_n"/>
        <pair ddi="true" e1="DDI-DrugBank.d514.s1.e0" e2="DDI-DrugBank.d514.s1.e1" id="DDI-DrugBank.d514.s1.p0" type="effect"/>
    </sentence>
    <sentence id="DDI-DrugBank.d514.s2" text="Toxicology studies of heroin-related deaths reveal frequent involvement of other central nervous system depressants, including alcohol, benzodiazepines such as diazepam (Valium), and, to a rising degree, methadone.">
