    <sentence id="DDI-DrugBank.d129.s0" text="Androgens may increase sensitivity to oral anticoagulahts.">
        <entity charOffset="0-8" id="DDI-DrugBank.d129.s0.e0" text="Androgens" type="group"/>
    </sentence>
    <sentence id="DDI-DrugBank.d129.s1" text="Dosage of the anticoagulant may require reduction in order to maintain satisfactory therapeutic hypoprothrombinemia.">
        <entity charOffset="14-26" id="DDI-DrugBank.d129.s1.e0" text="anticoagulant" type="group"/>
    </sentence>
    <sentence id="DDI-DrugBank.d129.s2" text="Concurrent administration of oxyphenbutazone and androgens may result in elevated serum levels of oxyphenbutazone.">
        <entity charOffset="29-43" id="DDI-DrugBank.d129.s2.e0" text="oxyphenbutazone" type="drug"/>
