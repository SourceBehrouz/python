    <sentence id="DDI-DrugBank.d386.s24" text="Both the sedative and anticholinergic effects of the major tranquilizers are also additive to those of desipramine.">
        <entity charOffset="53-71" id="DDI-DrugBank.d386.s24.e0" text="major tranquilizers" type="group"/>
        <entity charOffset="103-113" id="DDI-DrugBank.d386.s24.e1" text="desipramine" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d386.s24.e0" e2="DDI-DrugBank.d386.s24.e1" id="DDI-DrugBank.d386.s24.p0" type="effect"/>
    </sentence>
    <sentence id="DDI-DrugBank.d386.s25" text="5."/>
    <sentence id="DDI-DrugBank.d386.s26" text="Concurrent administration of cimetidine and tricyclic antidepressants can produce clinically significant increases in the plasma levels of the tricyclic antidepressants.">
        <entity charOffset="29-38" id="DDI-DrugBank.d386.s26.e0" text="cimetidine" type="drug"/>
        <entity charOffset="44-68" id="DDI-DrugBank.d386.s26.e1" text="tricyclic antidepressants" type="group"/>
        <entity charOffset="143-167" id="DDI-DrugBank.d386.s26.e2" text="tricyclic antidepressants" type="group"/>
        <pair ddi="true" e1="DDI-DrugBank.d386.s26.e0" e2="DDI-DrugBank.d386.s26.e1" id="DDI-DrugBank.d386.s26.p0" type="mechanism"/>
        <pair ddi="false" e1="DDI-DrugBank.d386.s26.e0" e2="DDI-DrugBank.d386.s26.e2" id="DDI-DrugBank.d386.s26.p1"/>
        <pair ddi="false" e1="DDI-DrugBank.d386.s26.e1" e2="DDI-DrugBank.d386.s26.e2" id="DDI-DrugBank.d386.s26.p2"/>
    </sentence>
    <sentence id="DDI-DrugBank.d386.s27" text="Conversely, decreases in plasma levels of the tricyclic antidepressants have been reported upon discontinuation of cimetidine which may result in the loss of the therapeutic efficacy of the tricyclic antidepressant 6.">
        <entity charOffset="46-70" id="DDI-DrugBank.d386.s27.e0" text="tricyclic antidepressants" type="group"/>
        <entity charOffset="115-124" id="DDI-DrugBank.d386.s27.e1" text="cimetidine" type="drug"/>
        <entity charOffset="190-213" id="DDI-DrugBank.d386.s27.e2" text="tricyclic antidepressant" type="group"/>
        <pair ddi="true" e1="DDI-DrugBank.d386.s27.e0" e2="DDI-DrugBank.d386.s27.e1" id="DDI-DrugBank.d386.s27.p0" type="mechanism"/>
        <pair ddi="false" e1="DDI-DrugBank.d386.s27.e0" e2="DDI-DrugBank.d386.s27.e2" id="DDI-DrugBank.d386.s27.p1"/>
        <pair ddi="false" e1="DDI-DrugBank.d386.s27.e1" e2="DDI-DrugBank.d386.s27.e2" id="DDI-DrugBank.d386.s27.p2"/>
    </sentence>
    <sentence id="DDI-DrugBank.d386.s28" text="There have been greater than two-fold increases of previously stable plasma levels of tricyclic antidepressants when fluoxetine has been administered in combination with these agents.">
        <entity charOffset="86-110" id="DDI-DrugBank.d386.s28.e0" text="tricyclic antidepressants" type="group"/>
        <entity charOffset="117-126" id="DDI-DrugBank.d386.s28.e1" text="fluoxetine" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d386.s28.e0" e2="DDI-DrugBank.d386.s28.e1" id="DDI-DrugBank.d386.s28.p0" type="mechanism"/>
    </sentence>
