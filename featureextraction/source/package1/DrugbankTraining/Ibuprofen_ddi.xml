    <sentence id="DDI-DrugBank.d415.s2" text="Aspirin: Animal studies wshow that aspirin given with nonsteroidal anti-inflammatory agents, including ibuprofen, yields a net decrease in anti-inflammatory activity with lowered blood levels of the non-aspirin drug.">
        <entity charOffset="0-6" id="DDI-DrugBank.d415.s2.e0" text="Aspirin" type="brand"/>
        <entity charOffset="35-41" id="DDI-DrugBank.d415.s2.e1" text="aspirin" type="brand"/>
        <entity charOffset="54-90" id="DDI-DrugBank.d415.s2.e2" text="nonsteroidal anti-inflammatory agents" type="group"/>
        <entity charOffset="103-111" id="DDI-DrugBank.d415.s2.e3" text="ibuprofen" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d415.s2.e0" e2="DDI-DrugBank.d415.s2.e1" id="DDI-DrugBank.d415.s2.p0"/>
