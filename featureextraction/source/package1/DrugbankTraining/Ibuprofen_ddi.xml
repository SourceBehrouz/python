    <sentence id="DDI-DrugBank.d415.s0" text="Coumarin-Type Anticoagulants: Several short-term controlled studies failed to wshow that ibuprofen significantly affected prothrombin times or a variety of other clotting factors when administered to individuals on coumarin-type anticoagulants.">
        <entity charOffset="0-27" id="DDI-DrugBank.d415.s0.e0" text="Coumarin-Type Anticoagulants" type="group"/>
        <entity charOffset="89-97" id="DDI-DrugBank.d415.s0.e1" text="ibuprofen" type="drug"/>
        <entity charOffset="215-242" id="DDI-DrugBank.d415.s0.e2" text="coumarin-type anticoagulants" type="group"/>
        <pair ddi="false" e1="DDI-DrugBank.d415.s0.e0" e2="DDI-DrugBank.d415.s0.e1" id="DDI-DrugBank.d415.s0.p0"/>
