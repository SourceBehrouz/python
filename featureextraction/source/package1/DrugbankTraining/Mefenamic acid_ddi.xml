    <sentence id="DDI-DrugBank.d400.s2" text="This may indicate that they could enhance the toxicity of methotrexate.">
        <entity charOffset="58-69" id="DDI-DrugBank.d400.s2.e0" text="methotrexate" type="drug"/>
    </sentence>
    <sentence id="DDI-DrugBank.d400.s3" text="Caution should be used when NSAIDs are administered concomitantly with methotrexate.">
        <entity charOffset="28-33" id="DDI-DrugBank.d400.s3.e0" text="NSAIDs" type="group"/>
        <entity charOffset="71-82" id="DDI-DrugBank.d400.s3.e1" text="methotrexate" type="drug"/>
