    <sentence id="DDI-DrugBank.d400.s7" text="This response has been attributed to inhibition of renal prostaglandin synthesis."/>
    <sentence id="DDI-DrugBank.d400.s8" text="During concomitant therapy of Ponstel with furosemide, the patient should be observed closely for signs of renal failure, as well as to assure diuretic efficacy.">
        <entity charOffset="30-36" id="DDI-DrugBank.d400.s8.e0" text="Ponstel" type="brand"/>
        <entity charOffset="43-52" id="DDI-DrugBank.d400.s8.e1" text="furosemide" type="drug"/>
