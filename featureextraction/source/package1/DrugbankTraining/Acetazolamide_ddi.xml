<?xml version="1.0" encoding="UTF-8" standalone="no"?><document id="DDI-DrugBank.d368">
    <sentence id="DDI-DrugBank.d368.s0" text="DIAMOX modifies phenytoin metabolism with increased serum levels of phenytoin.">
        <entity charOffset="0-5" id="DDI-DrugBank.d368.s0.e0" text="DIAMOX" type="brand"/>
        <entity charOffset="16-24" id="DDI-DrugBank.d368.s0.e1" text="phenytoin" type="drug"/>
        <entity charOffset="68-76" id="DDI-DrugBank.d368.s0.e2" text="phenytoin" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d368.s0.e0" e2="DDI-DrugBank.d368.s0.e1" id="DDI-DrugBank.d368.s0.p0" type="mechanism"/>
        <pair ddi="true" e1="DDI-DrugBank.d368.s0.e0" e2="DDI-DrugBank.d368.s0.e2" id="DDI-DrugBank.d368.s0.p1" type="mechanism"/>
        <pair ddi="false" e1="DDI-DrugBank.d368.s0.e1" e2="DDI-DrugBank.d368.s0.e2" id="DDI-DrugBank.d368.s0.p2"/>
    </sentence>
    <sentence id="DDI-DrugBank.d368.s1" text="This may increase or enhance the occurrence of osteomalacia in some patients receiving chronic phenytoin therapy.">
        <entity charOffset="95-103" id="DDI-DrugBank.d368.s1.e0" text="phenytoin" type="drug"/>
    </sentence>
    <sentence id="DDI-DrugBank.d368.s2" text="Caution is advised in patients receiving chronic concomitant therapy."/>
    <sentence id="DDI-DrugBank.d368.s3" text="By decreasing the gastrointestinal absorption of primidone, DIAMOX may decrease serum concentrations of primidone and its metabolites, with a consequent possible decrease in anticonvulsant effect.">
        <entity charOffset="49-57" id="DDI-DrugBank.d368.s3.e0" text="primidone" type="drug"/>
        <entity charOffset="60-65" id="DDI-DrugBank.d368.s3.e1" text="DIAMOX" type="brand"/>
        <entity charOffset="104-112" id="DDI-DrugBank.d368.s3.e2" text="primidone" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d368.s3.e0" e2="DDI-DrugBank.d368.s3.e1" id="DDI-DrugBank.d368.s3.p0"/>
        <pair ddi="false" e1="DDI-DrugBank.d368.s3.e0" e2="DDI-DrugBank.d368.s3.e2" id="DDI-DrugBank.d368.s3.p1"/>
        <pair ddi="true" e1="DDI-DrugBank.d368.s3.e1" e2="DDI-DrugBank.d368.s3.e2" id="DDI-DrugBank.d368.s3.p2" type="mechanism"/>
    </sentence>
    <sentence id="DDI-DrugBank.d368.s4" text="Caution is advised when beginning, discontinuing, or changing the dose of DIAMOX in patients receiving primidone.">
        <entity charOffset="74-79" id="DDI-DrugBank.d368.s4.e0" text="DIAMOX" type="brand"/>
        <entity charOffset="103-111" id="DDI-DrugBank.d368.s4.e1" text="primidone" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d368.s4.e0" e2="DDI-DrugBank.d368.s4.e1" id="DDI-DrugBank.d368.s4.p0" type="advise"/>
    </sentence>
    <sentence id="DDI-DrugBank.d368.s5" text="Because of possible additive effects with other carbonic anhydrase inhibitors, concomitant use is not advisable.">
        <entity charOffset="48-76" id="DDI-DrugBank.d368.s5.e0" text="carbonic anhydrase inhibitors" type="group"/>
    <negationtags>Because of possible additive effects with other carbonic anhydrase inhibitors, concomitant use is &lt;scope&gt; &lt;cue&gt; not &lt;/cue&gt; advisable. &lt;/scope&gt;</negationtags></sentence>
    <sentence id="DDI-DrugBank.d368.s6" text="Acetazolamide may increase the effects of other folic acid antagonists.">
        <entity charOffset="0-12" id="DDI-DrugBank.d368.s6.e0" text="Acetazolamide" type="drug"/>
        <entity charOffset="48-69" id="DDI-DrugBank.d368.s6.e1" text="folic acid antagonists" type="group"/>
        <pair ddi="true" e1="DDI-DrugBank.d368.s6.e0" e2="DDI-DrugBank.d368.s6.e1" id="DDI-DrugBank.d368.s6.p0" type="effect"/>
    </sentence>
    <sentence id="DDI-DrugBank.d368.s7" text="Acetazolamide may increase or decrease blood glucose levels.">
        <entity charOffset="0-12" id="DDI-DrugBank.d368.s7.e0" text="Acetazolamide" type="drug"/>
    </sentence>
    <sentence id="DDI-DrugBank.d368.s8" text="Consideration should be taken in patients being treated with antidiabetic agents.">
        <entity charOffset="61-72" id="DDI-DrugBank.d368.s8.e0" text="antidiabetic" type="drug"/>
    </sentence>
    <sentence id="DDI-DrugBank.d368.s9" text="Acetazolamide decreases urinary excretion of amphetamine and may enhance the magnitude and duration of their effect.">
        <entity charOffset="0-12" id="DDI-DrugBank.d368.s9.e0" text="Acetazolamide" type="drug"/>
        <entity charOffset="45-55" id="DDI-DrugBank.d368.s9.e1" text="amphetamine" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d368.s9.e0" e2="DDI-DrugBank.d368.s9.e1" id="DDI-DrugBank.d368.s9.p0" type="mechanism"/>
    </sentence>
    <sentence id="DDI-DrugBank.d368.s10" text="Acetazolamide reduces urinary excretion of quinidine and may enhance its effect.">
        <entity charOffset="0-12" id="DDI-DrugBank.d368.s10.e0" text="Acetazolamide" type="drug"/>
        <entity charOffset="43-51" id="DDI-DrugBank.d368.s10.e1" text="quinidine" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d368.s10.e0" e2="DDI-DrugBank.d368.s10.e1" id="DDI-DrugBank.d368.s10.p0" type="mechanism"/>
    </sentence>
    <sentence id="DDI-DrugBank.d368.s11" text="Acetazolamide may prevent the urinary antiseptic effect of methenamine.">
        <entity charOffset="0-12" id="DDI-DrugBank.d368.s11.e0" text="Acetazolamide" type="drug"/>
        <entity charOffset="59-69" id="DDI-DrugBank.d368.s11.e1" text="methenamine" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d368.s11.e0" e2="DDI-DrugBank.d368.s11.e1" id="DDI-DrugBank.d368.s11.p0" type="effect"/>
    </sentence>
    <sentence id="DDI-DrugBank.d368.s12" text="Acetazolamide increases lithium excretion and the lithium may be decreased.">
        <entity charOffset="0-12" id="DDI-DrugBank.d368.s12.e0" text="Acetazolamide" type="drug"/>
        <entity charOffset="24-30" id="DDI-DrugBank.d368.s12.e1" text="lithium" type="drug"/>
        <entity charOffset="50-56" id="DDI-DrugBank.d368.s12.e2" text="lithium" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d368.s12.e0" e2="DDI-DrugBank.d368.s12.e1" id="DDI-DrugBank.d368.s12.p0" type="mechanism"/>
        <pair ddi="false" e1="DDI-DrugBank.d368.s12.e0" e2="DDI-DrugBank.d368.s12.e2" id="DDI-DrugBank.d368.s12.p1"/>
        <pair ddi="false" e1="DDI-DrugBank.d368.s12.e1" e2="DDI-DrugBank.d368.s12.e2" id="DDI-DrugBank.d368.s12.p2"/>
    </sentence>
    <sentence id="DDI-DrugBank.d368.s13" text="Acetazolamide and sodium bicarbonate used concurrently increases the risk of renal calculus formation.">
        <entity charOffset="0-12" id="DDI-DrugBank.d368.s13.e0" text="Acetazolamide" type="drug"/>
        <entity charOffset="18-35" id="DDI-DrugBank.d368.s13.e1" text="sodium bicarbonate" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d368.s13.e0" e2="DDI-DrugBank.d368.s13.e1" id="DDI-DrugBank.d368.s13.p0" type="effect"/>
    </sentence>
    <sentence id="DDI-DrugBank.d368.s14" text="Acetazolamide may elevate cyclosporine levels.">
        <entity charOffset="0-12" id="DDI-DrugBank.d368.s14.e0" text="Acetazolamide" type="drug"/>
        <entity charOffset="26-37" id="DDI-DrugBank.d368.s14.e1" text="cyclosporine" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d368.s14.e0" e2="DDI-DrugBank.d368.s14.e1" id="DDI-DrugBank.d368.s14.p0" type="mechanism"/>
    </sentence>
