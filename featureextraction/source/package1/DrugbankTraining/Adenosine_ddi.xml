    </sentence>
    <sentence id="DDI-DrugBank.d226.s5" text="In the presence of these methylxanthines, larger doses of adenosine may be required or adenosine may not be effective.">
        <entity charOffset="25-39" id="DDI-DrugBank.d226.s5.e0" text="methylxanthines" type="group"/>
        <entity charOffset="58-66" id="DDI-DrugBank.d226.s5.e1" text="adenosine" type="drug"/>
        <entity charOffset="87-95" id="DDI-DrugBank.d226.s5.e2" text="adenosine" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d226.s5.e0" e2="DDI-DrugBank.d226.s5.e1" id="DDI-DrugBank.d226.s5.p0" type="advise"/>
