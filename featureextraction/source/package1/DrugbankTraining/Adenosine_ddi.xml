    <sentence id="DDI-DrugBank.d226.s1" text="Digoxin and verapamil use may be rarely associated with ventricular fibrillation when combined with Adenocard.">
        <entity charOffset="0-6" id="DDI-DrugBank.d226.s1.e0" text="Digoxin" type="drug"/>
        <entity charOffset="12-20" id="DDI-DrugBank.d226.s1.e1" text="verapamil" type="drug"/>
