    <sentence id="DDI-DrugBank.d241.s1" text="When other antiplatelet agents or anticoagulants are used concomitantly, there is the potential for FLOLAN to increase the risk of bleeding.">
        <entity charOffset="11-29" id="DDI-DrugBank.d241.s1.e0" text="antiplatelet agents" type="group"/>
        <entity charOffset="34-47" id="DDI-DrugBank.d241.s1.e1" text="anticoagulants" type="group"/>
