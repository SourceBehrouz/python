    <sentence id="DDI-DrugBank.d373.s3" text="Extended Release Tablets: Administration of nifedipine with digoxin increased digoxin levels in 9 of 12 normal volunteers.">
        <entity charOffset="44-53" id="DDI-DrugBank.d373.s3.e0" text="nifedipine" type="drug"/>
        <entity charOffset="60-66" id="DDI-DrugBank.d373.s3.e1" text="digoxin" type="drug"/>
        <entity charOffset="78-84" id="DDI-DrugBank.d373.s3.e2" text="digoxin" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d373.s3.e0" e2="DDI-DrugBank.d373.s3.e1" id="DDI-DrugBank.d373.s3.p0"/>
