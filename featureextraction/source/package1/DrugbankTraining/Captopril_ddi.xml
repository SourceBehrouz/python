        <pair ddi="false" e1="DDI-DrugBank.d175.s18.e1" e2="DDI-DrugBank.d175.s18.e2" id="DDI-DrugBank.d175.s18.p4"/>
        <pair ddi="false" e1="DDI-DrugBank.d175.s18.e1" e2="DDI-DrugBank.d175.s18.e3" id="DDI-DrugBank.d175.s18.p5"/>
        <pair ddi="false" e1="DDI-DrugBank.d175.s18.e1" e2="DDI-DrugBank.d175.s18.e4" id="DDI-DrugBank.d175.s18.p6"/>
        <pair ddi="false" e1="DDI-DrugBank.d175.s18.e2" e2="DDI-DrugBank.d175.s18.e3" id="DDI-DrugBank.d175.s18.p7"/>
        <pair ddi="false" e1="DDI-DrugBank.d175.s18.e2" e2="DDI-DrugBank.d175.s18.e4" id="DDI-DrugBank.d175.s18.p8"/>
        <pair ddi="true" e1="DDI-DrugBank.d175.s18.e3" e2="DDI-DrugBank.d175.s18.e4" id="DDI-DrugBank.d175.s18.p9" type="effect"/>
    </sentence>
    <sentence id="DDI-DrugBank.d175.s19" text="These drugs should be coad-ministered with caution and frequent monitoring of serum lithium levels is recommended.">
        <entity charOffset="84-90" id="DDI-DrugBank.d175.s19.e0" text="lithium" type="drug"/>
    </sentence>
    <sentence id="DDI-DrugBank.d175.s20" text="If a diuretic is also used, it may increase the risk of lithium toxicity.">
        <entity charOffset="5-12" id="DDI-DrugBank.d175.s20.e0" text="diuretic" type="group"/>
        <entity charOffset="56-62" id="DDI-DrugBank.d175.s20.e1" text="lithium" type="drug"/>
        <pair ddi="true" e1="DDI-DrugBank.d175.s20.e0" e2="DDI-DrugBank.d175.s20.e1" id="DDI-DrugBank.d175.s20.p0" type="effect"/>
    </sentence>
    <sentence id="DDI-DrugBank.d175.s21" text="Cardiac Glycosides: In a study of young healthy male subjects no evidence of a direct pharmacokinetic captopril-digoxin interaction could be found.">
        <entity charOffset="0-17" id="DDI-DrugBank.d175.s21.e0" text="Cardiac Glycosides" type="group"/>
        <entity charOffset="102-110" id="DDI-DrugBank.d175.s21.e1" text="captopril" type="drug"/>
        <entity charOffset="112-118" id="DDI-DrugBank.d175.s21.e2" text="digoxin" type="drug"/>
