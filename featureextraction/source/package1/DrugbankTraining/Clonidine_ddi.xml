    <sentence id="DDI-DrugBank.d495.s3" text="Narcotic analgesics may potentiate the hypotensive effects of clonidine.">
        <entity charOffset="0-18" id="DDI-DrugBank.d495.s3.e0" text="Narcotic analgesics" type="group"/>
        <entity charOffset="62-70" id="DDI-DrugBank.d495.s3.e1" text="clonidine" type="drug"/>
