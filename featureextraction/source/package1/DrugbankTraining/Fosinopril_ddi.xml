    <sentence id="DDI-DrugBank.d176.s0" text="Diuretics: Patients on diuretics, especially those with intravascular volume depletion, may occasionally experience an excessive reduction of blood pressure after initiation of therapy with fosinopril sodium.">
        <entity charOffset="0-8" id="DDI-DrugBank.d176.s0.e0" text="Diuretics" type="group"/>
        <entity charOffset="23-31" id="DDI-DrugBank.d176.s0.e1" text="diuretics" type="group"/>
        <entity charOffset="190-206" id="DDI-DrugBank.d176.s0.e2" text="fosinopril sodium" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d176.s0.e0" e2="DDI-DrugBank.d176.s0.e1" id="DDI-DrugBank.d176.s0.p0"/>
