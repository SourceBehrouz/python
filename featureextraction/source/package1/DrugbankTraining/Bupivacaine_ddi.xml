    <sentence id="DDI-DrugBank.d153.s1" text="Concurrent use of these agents should generally be avoided."/>
    <sentence id="DDI-DrugBank.d153.s2" text="In situations in which concurrent therapy is necessary, careful patient monitoring is essential."/>
    <sentence id="DDI-DrugBank.d153.s3" text="Concurrent administration of vasopressor drugs and of ergot-type oxytocic drugs may cause severe, persistent hypertension or cerebrovascular accidents.">
        <entity charOffset="29-45" id="DDI-DrugBank.d153.s3.e0" text="vasopressor drugs" type="group"/>
        <entity charOffset="54-78" id="DDI-DrugBank.d153.s3.e1" text="ergot-type oxytocic drugs" type="group"/>
        <pair ddi="false" e1="DDI-DrugBank.d153.s3.e0" e2="DDI-DrugBank.d153.s3.e1" id="DDI-DrugBank.d153.s3.p0"/>
    </sentence>
    <sentence id="DDI-DrugBank.d153.s4" text="Phenothiazines and butyrophenones may reduce or reverse the pressor effect of epinephrine.">
        <entity charOffset="0-13" id="DDI-DrugBank.d153.s4.e0" text="Phenothiazines" type="group"/>
