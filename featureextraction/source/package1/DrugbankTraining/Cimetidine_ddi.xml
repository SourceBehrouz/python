    <sentence id="DDI-DrugBank.d171.s1" text="Clinically significant effects have been reported with the warfarin anticoagulants;">
        <entity charOffset="59-81" id="DDI-DrugBank.d171.s1.e0" text="warfarin anticoagulants" type="group"/>
    </sentence>
    <sentence id="DDI-DrugBank.d171.s2" text="therefore, close monitoring of prothrombin time is recommended, and adjustment of the anticoagulant dose may be necessary when Tagamet is administered concomitantly.">
        <entity charOffset="86-98" id="DDI-DrugBank.d171.s2.e0" text="anticoagulant" type="group"/>
        <entity charOffset="127-133" id="DDI-DrugBank.d171.s2.e1" text="Tagamet" type="brand"/>
        <pair ddi="true" e1="DDI-DrugBank.d171.s2.e0" e2="DDI-DrugBank.d171.s2.e1" id="DDI-DrugBank.d171.s2.p0" type="advise"/>
    </sentence>
    <sentence id="DDI-DrugBank.d171.s3" text="Interaction with phenytoin, lidocaine and theophylline has also been reported to produce adverse clinical effects.">
        <entity charOffset="17-25" id="DDI-DrugBank.d171.s3.e0" text="phenytoin" type="drug"/>
        <entity charOffset="28-36" id="DDI-DrugBank.d171.s3.e1" text="lidocaine" type="drug"/>
        <entity charOffset="42-53" id="DDI-DrugBank.d171.s3.e2" text="theophylline" type="drug"/>
        <pair ddi="false" e1="DDI-DrugBank.d171.s3.e0" e2="DDI-DrugBank.d171.s3.e1" id="DDI-DrugBank.d171.s3.p0"/>
